fun pad2(n: int): string {
  if (n < 10) {
    return ("0" + str(n))
  }
  return str(n)
}
fun weekdayName(z: int): string {
  let names: list<string> = ["Sunday", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday"]
  return names[(((z + 4)) % 7)]
}
