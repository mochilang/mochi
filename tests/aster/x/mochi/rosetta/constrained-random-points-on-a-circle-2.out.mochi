let nPts: int = 100
let rMin: int = 10
let rMax: int = 15
let span: int = ((rMax + 1) + rMax)
var poss: list<list<int>>
let min2: int = (rMin * rMin)
let max2: int = (rMax * rMax)
var y: int = -rMax
while (y <= rMax) {
  var x: int = -rMax
  while (x <= rMax) {
    let r2: int = ((x * x) + (y * y))
    if ((r2 >= min2) && (r2 <= max2)) {
      poss = append(poss, [x, y])
    }
    x = (x + 1)
  }
  y = (y + 1)
}
print((str(len(poss)) + " possible points"))
var rows: list<list<string>>
var r: int = 0
while (r < span) {
  var row: list<string>
  var c: int = 0
  while (c < (span * 2)) {
    row = append(row, " ")
    c = (c + 1)
  }
  rows = append(rows, row)
  r = (r + 1)
}
var u: int = 0
var seen: map<string, bool>
var n: int = 0
while (n < nPts) {
  var i: int = (now() % len(poss))
  let x: int = poss[i][0]
  let yy: int = poss[i][1]
  let row: int = (yy + rMax)
  let col: int = (((x + rMax)) * 2)
  rows[row][col] = "*"
  let key: string = ((str(row) + ",") + str(col))
  if !seen[key] {
    seen[key] = true
    u = (u + 1)
  }
  n = (n + 1)
}
var i2: int = 0
while (i2 < span) {
  var line: string = ""
  var j: int = 0
  while (j < (span * 2)) {
    line = (line + rows[i2][j])
    j = (j + 1)
  }
  print(line)
  i2 = (i2 + 1)
}
print((str(u) + " unique points"))
