fun pfacSum(i: int): int {
  var sum: int = 0
  var p: int = 1
  while (p <= (i / 2)) {
    if ((i % p) == 0) {
      sum = (sum + p)
    }
    p = (p + 1)
  }
  return sum
}
fun main() {
  var d: int = 0
  var a: int = 0
  var pnum: int = 0
  var i: int = 1
  while (i <= 20000) {
    let j: int = pfacSum(i)
    if (j < i) {
      d = (d + 1)
    }
    if (j == i) {
      pnum = (pnum + 1)
    }
    if (j > i) {
      a = (a + 1)
    }
    i = (i + 1)
  }
  print((("There are " + str(d)) + " deficient numbers between 1 and 20000"))
  print((("There are " + str(a)) + " abundant numbers  between 1 and 20000"))
  print((("There are " + str(pnum)) + " perfect numbers between 1 and 20000"))
}
main()
