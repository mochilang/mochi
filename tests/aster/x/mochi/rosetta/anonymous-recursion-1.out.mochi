fun fib(n: int): int {
  if (n < 2) {
    return n
  }
  var a: int = 0
  var b: int = 1
  var i: int = 1
  while (i < n) {
    let t: int = (a + b)
    a = b
    b = t
    i = (i + 1)
  }
  return b
}
fun main() {
  for n in [0, 1, 2, 3, 4, 5, 10, 40, -1] {
    if (n < 0) {
      print("fib undefined for negative numbers")
    } else {
      print(((("fib " + str(n)) + " = ") + str(fib(n))))
    }
  }
}
main()
