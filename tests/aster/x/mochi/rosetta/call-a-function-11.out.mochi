fun zeroval(ival: int): int {
  var x: int = ival
  x = 0
  return x
}
fun zeroptr(ref: list<int>): void {
  ref[0] = 0
}
fun main(): void {
  var i: int = 1
  print(("initial: " + str(i)))
  let tmp: int = zeroval(i)
  print(("zeroval: " + str(i)))
  var box: list<int> = [i]
  zeroptr(box)
  i = box[0]
  print(("zeroptr: " + str(i)))
  print("pointer: 0")
}
main()
