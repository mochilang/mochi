fun main(): void {
  let philosophers: list<string> = ["Aristotle", "Kant", "Spinoza", "Marx", "Russell"]
  let hunger: int = 3
  print("table empty")
  for p in philosophers {
    print((p + " seated"))
  }
  var idx: int = 0
  while (idx < len(philosophers)) {
    let name: string = philosophers[idx]
    var h: int = 0
    while (h < hunger) {
      print((name + " hungry"))
      print((name + " eating"))
      print((name + " thinking"))
      h = (h + 1)
    }
    print((name + " satisfied"))
    print((name + " left the table"))
    idx = (idx + 1)
  }
  print("table empty")
}
main()
