type DotResult {
  value: int
  ok: bool
}
fun dot(x: list<int>, y: list<int>): DotResult {
  if (len(x) != len(y)) {
    return DotResult {value: 0, ok: false}
  }
  var sum: int = 0
  var i: int = 0
  while (i < len(x)) {
    sum = (sum + (x[i] * y[i]))
    i = (i + 1)
  }
  return DotResult {value: sum, ok: true}
}
fun main(): void {
  let r: DotResult = dot([1, 3, -5], [4, -2, -1])
  if !r.ok {
    print("incompatible lengths")
  } else {
    print(str(r.value))
  }
}
main()
