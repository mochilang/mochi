fun nextPrime(primes: list<int>, start: int): int {
  var n: int = start
  while true {
    var isP: bool = true
    var i: int = 0
    while (i < len(primes)) {
      let p: int = primes[i]
      if ((p * p) > n) {
        break
      }
      if ((n % p) == 0) {
        isP = false
        break
      }
      i = (i + 1)
    }
    if isP {
      return n
    }
    n = (n + 2)
  }
}
fun main(): void {
  var primes: list<int> = [2]
  var cand: int = 3
  while (len(primes) < 10000) {
