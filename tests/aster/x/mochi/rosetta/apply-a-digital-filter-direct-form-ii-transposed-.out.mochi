fun applyFilter(input: list<float>, a: list<float>, b: list<float>): list<float> {
  var out: list<float>
  let scale: float = (1 / a[0])
  var i: int = 0
  while (i < len(input)) {
    var tmp: float = 0
    var j: int = 0
    while ((j <= i) && (j < len(b))) {
      tmp = (tmp + (b[j] * input[(i - j)]))
      j = (j + 1)
    }
    j = 0
    while ((j < i) && ((j + 1) < len(a))) {
      tmp = (tmp - (a[(j + 1)] * out[((i - j) - 1)]))
      j = (j + 1)
    }
    out = append(out, (tmp * scale))
    i = (i + 1)
  }
  return out
}
let a: list<float> = [1, -2.7756e-16, 0.33333333, -1.85e-17]
let b: list<float> = [0.16666667, 0.5, 0.5, 0.16666667]
let sig: list<float> = [-0.917843918645, 0.141984778794, 1.20536903482, 0.190286794412, -0.662370894973, -1.00700480494, -0.404707073677, 0.800482325044, 0.743500089861, 1.01090520172, 0.741527555207, 0.277841675195, 0.400833448236, -0.2085993586, -0.172842103641, -0.134316096293, 0.0259303398477, 0.490105989562, 0.549391221511, 0.9047198589]
let res: list<float> = applyFilter(sig, a, b)
var k: int = 0
while (k < len(res)) {
