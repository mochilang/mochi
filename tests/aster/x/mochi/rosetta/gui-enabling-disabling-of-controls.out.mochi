type State {
  entry: bool
  inc: bool
  dec: bool
}
fun state(v: int): State {
  return State {entry: (v == 0), inc: (v < 10), dec: (v > 0)}
}
fun printState(v: int): void {
  let s: State = state(v)
  print(((((((("value=" + str(v)) + " entry=") + str(s.entry)) + " inc=") + str(s.inc)) + " dec=") + str(s.dec)))
}
fun main(): void {
  var v: int = 0
  printState(v)
  while true {
    let s: State = state(v)
    if !s.inc {
      break
    }
    v = (v + 1)
    printState(v)
  }
  while true {
    let s: State = state(v)
    if !s.dec {
      break
    }
    v = (v - 1)
    printState(v)
  }
}
main()
