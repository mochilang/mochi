fun padLeft(s: string, w: int): string {
  var res: string = ""
  var n: int = (w - len(s))
  while (n > 0) {
    res = (res + " ")
    n = (n - 1)
  }
  return (res + s)
}
fun padRight(s: string, w: int): string {
  var out: string = s
  var i: int = len(s)
  while (i < w) {
    out = (out + " ")
    i = (i + 1)
  }
  return out
}
fun indexOf(s: string, ch: string): int {
  var i: int = 0
  while (i < len(s)) {
    if (substring(s, i, (i + 1)) == ch) {
      return i
    }
    i = (i + 1)
  }
  return -1
}
fun format2(f: float): string {
  var s: string = str(f)
  let idx: int = indexOf(s, ".")
  if (idx < 0) {
    s = (s + ".00")
  } else {
    var need: int = (idx + 3)
    if (len(s) > need) {
      s = substring(s, 0, need)
    } else {
      while (len(s) < need) {
        s = (s + "0")
      }
    }
  }
  return s
}
fun cpx(h: float): int {
  var x: int = ((((h / 11.25)) + 0.5)) as int
  x = (x % 32)
  if (x < 0) {
    x = (x + 32)
  }
  return x
}
