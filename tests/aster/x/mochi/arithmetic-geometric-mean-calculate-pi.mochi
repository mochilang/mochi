fun abs(x: float): float {
  if (x < 0) {
    return -x
  }
  return x
}
fun sqrtApprox(x: float): float {
  var guess = x
  var i = 0
  while (i < 20) {
    guess = (((guess + (x / guess))) / 2)
    i = (i + 1)
  }
  return guess
}
fun agmPi(): float {
  var a = 1
  var g = (1 / sqrtApprox(2))
  var sum = 0
  var pow = 2
  while (abs((a - g)) > 1e-15) {
    var t = (((a + g)) / 2)
    var u = sqrtApprox((a * g))
    a = t
    g = u
    pow = (pow * 2)
    var diff = ((a * a) - (g * g))
    sum = (sum + (diff * pow))
  }
  var pi = (((4 * a) * a) / ((1 - sum)))
  return pi
}
fun main() {
  print(str(agmPi()))
}
main()
