#lang racket/base
(require racket/list racket/string json)

(define people
  (list
    (hash "name" "Alice" "age" 30 "email" "alice@example.com")
    (hash "name" "Bob" "age" 15 "email" "bob@example.com")
    (hash "name" "Charlie" "age" 20 "email" "charlie@example.com")))

