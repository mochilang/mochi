// Solution for SPOJ JRIDE - Jill Rides Again
// https://www.spoj.com/problems/JRIDE/

fun main() {
  let tStr = input()
  if tStr == "" { return }
  let t = tStr as int
  var r = 1
  while r <= t {
    let sStr = input()
    let s = sStr as int
    var i = 1
    var current = 0
    var best = 0
    var bestStart = 0
    var bestEnd = 0
    var currentStart = 1
    while i < s {
      let nStr = input()
      let val = nStr as int
      current = current + val
      let length = i + 1 - currentStart
      let bestLen = bestEnd - bestStart
      if current > best || (current == best && length > bestLen) {
        best = current
        bestStart = currentStart
        bestEnd = i + 1
      }
      if current < 0 {
        current = 0
        currentStart = i + 1
      }
      i = i + 1
    }
    if best > 0 {
      print("The nicest part of route " + str(r) + " is between stops " + str(bestStart) + " and " + str(bestEnd))
    } else {
      print("Route " + str(r) + " has no nice parts")
    }
    r = r + 1
  }
}

main()
