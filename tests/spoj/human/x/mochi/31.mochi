// Solution for SPOJ MUL - Fast Multiplication
// https://www.spoj.com/problems/MUL/

fun split_two(s: string): list<string> {
  var res: list<string> = []
  var cur = ""
  var i = 0
  while i < len(s) {
    let ch = substring(s, i, i+1)
    if ch == " " {
      if cur != "" {
        res = append(res, cur)
        cur = ""
      }
    } else {
      cur = cur + ch
    }
    i = i + 1
  }
  if cur != "" {
    res = append(res, cur)
  }
  return res
}

fun main() {
  let tLine = input()
  if tLine == "" { return }
  let t = tLine as int
  var i = 0
  while i < t {
    let line = input()
    let parts = split_two(line)
    let a: bigint = parts[0] as bigint
    let b: bigint = parts[1] as bigint
    print(str(a * b))
    i = i + 1
  }
}

main()
