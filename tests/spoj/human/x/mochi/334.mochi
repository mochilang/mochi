// Solution for SPOJ PHDISP - The Philosophical Dispute
// https://www.spoj.com/problems/PHDISP/

fun splitTwo(s: string): list<string> {
  var res: list<string> = []
  var cur = ""
  var i = 0
  while i < len(s) {
    let ch = substring(s, i, i+1)
    if ch == " " {
      if cur != "" {
        res = append(res, cur)
        cur = ""
      }
    } else {
      cur = cur + ch
    }
    i = i + 1
  }
  if cur != "" {
    res = append(res, cur)
  }
  return res
}

fun format1(x: float): string {
  let scaled = x * 10.0 + 0.5
  let n = scaled as int
  let whole = n / 10
  let frac = n % 10
  return str(whole) + "." + str(frac)
}

fun main() {
  let tLine = input()
  if tLine == "" { return }
  let t = tLine as int
  var i = 0
  while i < t {
    let line = input()
    let parts = splitTwo(line)
    let x0 = parts[0] as float
    let y0 = parts[1] as float
    let epsLine = input() // unused, radius of the dot
    let tval = asin(y0) + 1.5 * 3.141592653589793
    print(format1(tval))
    i = i + 1
  }
}

