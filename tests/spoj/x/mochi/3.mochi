// Substring Check (Bug Funny)
// https://www.spoj.com/problems/SBSTR1

fun is_substring(a: string, b: string): int {
  let la = len(a)
  let lb = len(b)
  var i = 0
  while i + lb <= la {
    if a[i:i+lb] == b { return 1 }
    i = i + 1
  }
  return 0
}

fun solve(lines: list<string>): list<int> {
  var res: list<int> = []
  for line in lines {
    var parts: list<string> = []
    var cur = ""
    var i = 0
    while i < len(line) {
      let ch = line[i:i+1]
      if ch == " " {
        parts = append(parts, cur)
        cur = ""
      } else {
        cur = cur + ch
      }
      i = i + 1
    }
    parts = append(parts, cur)
    let a = parts[0]
    let b = parts[1]
    res = append(res, is_substring(a, b))
  }
  return res
}

let sample_input = [
  "1010110010 10110",
  "1110111011 10011",
]
for r in solve(sample_input) {
  print(r)
}
