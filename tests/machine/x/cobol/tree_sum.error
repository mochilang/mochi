// tree.mochi

// Algebraic data type for a binary tree of integers

type Tree =
  Leaf
  | Node(left: Tree, value: int, right: Tree)

fun sum_tree(t: Tree): int {
  return match t {
    Leaf => 0
    Node(left, value, right) => sum_tree(left) + value + sum_tree(right)
  }
}

