var count = 0
var i = 2
while i < 7 {
  var j = 1
  while j < 8 {
    if j != i {
      var k = 1
      while k < 8 {
        if k != i && k != j {
          if i + j + k == 12 {
            print("  " + str(i) + "         " + str(j) + "         " + str(k))
            count = count + 1
          }
        }
        k = k + 1
      }
    }
    j = j + 1
  }
  i = i + 2
}
print("")
print(str(count) + " valid combinations")
