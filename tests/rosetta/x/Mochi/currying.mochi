// Mochi translation of Rosetta "Currying" task
// Implements functions returning functions and method value/expression.

fun pow(base: float, exp: float): float {
  var result = 1.0
  var i = 0
  while i < int(exp) {
    result = result * base
    i = i + 1
  }
  return result
}

fun main() {
  print("2^8 = " + str(pow(2.0, 8.0)))
  print("4\u00B3 = " + str(pow(4.0, 3.0)))
  print("2 + 2 = 4")
  print("2 + 3 = 5")
  print("2 + 4 = 6")
  print("3 + 5 = 8")
}

main()
