fun sortFloat(xs: list<float>): list<float> {
  var arr = xs
  var n = len(arr)
  var i = 0
  while i < n {
    var j = 0
    while j < n - 1 {
      if arr[j] > arr[j+1] {
        let t = arr[j]
        arr[j] = arr[j+1]
        arr[j+1] = t
      }
      j = j + 1
    }
    i = i + 1
  }
  return arr
}

fun median(s: list<float>): float {
  let n = len(s)
  if n % 2 == 1 { return s[n/2] }
  return (s[n/2-1] + s[n/2]) / 2.0
}

fun fivenum(xs: list<float>): list<float> {
  var s = sortFloat(xs)
  let n = len(s)
  let q1 = s[(n-1)/4]
  let med = median(s)
  let q3 = s[(3*(n-1))/4]
  return [s[0], q1, med, q3, s[n-1]]
}

