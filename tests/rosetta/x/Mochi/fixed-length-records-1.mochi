// Simplified demonstration of reversing fixed length records
let records = [
  "abcdefgh",
  "ijklmnop",
  "qrstuvwx",
]

fun reverseStr(s: string): string {
  var out = ""
  var i = len(s) - 1
  while i >= 0 {
    out = out + s[i:i+1]
    i = i - 1
  }
  return out
}

for r in records {
  print(reverseStr(r))
}
