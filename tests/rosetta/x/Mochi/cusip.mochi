fun ord(ch: string): int {
  let upper = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
  if ch >= "0" && ch <= "9" { return parseIntStr(ch) + 48 }
  var idx = indexOf(upper, ch)
  if idx >= 0 { return 65 + idx }
  return 0
}

fun isCusip(s: string): bool {
  if len(s) != 9 { return false }
  var sum = 0
  var i = 0
  while i < 8 {
    let c = s[i:i+1]
    var v = 0
    if c >= "0" && c <= "9" {
      v = parseIntStr(c)
    } else if c >= "A" && c <= "Z" {
      v = ord(c) - 55
    } else if c == "*" {
      v = 36
    } else if c == "@" {
      v = 37
    } else if c == "#" {
      v = 38
    } else {
      return false
    }
    if i % 2 == 1 { v = v * 2 }
    sum = sum + v/10 + v%10
    i = i + 1
  }
  return parseIntStr(s[8:9]) == (10 - (sum % 10)) % 10
}

let candidates = ["037833100","17275R102","38259P508","594918104","68389X106","68389X105"]
for cand in candidates {
  var b = "incorrect"
  if isCusip(cand) { b = "correct" }
  print(cand + " -> " + b)
}
