fun pfacSum(i: int): int {
  var sum = 0
  var k = 1
  let limit = (i / 2) as int
  while k <= limit {
    if i % k == 0 {
      sum = sum + k
    }
    k = k + 1
  }
  return sum
}

fun main() {
  var d = 0
  var a = 0
  var p = 0
  var i = 1
  while i <= 20000 {
    let j = pfacSum(i)
    if j > i {
      a = a + 1
    } else {
      if j == i {
        p = p + 1
      } else {
        d = d + 1
      }
    }
    i = i + 1
  }
  print("There are " + str(d) + " deficient numbers between 1 and 20000")
  print("There are " + str(a) + " abundant numbers  between 1 and 20000")
  print("There are " + str(p) + " perfect numbers between 1 and 20000")
}

main()
