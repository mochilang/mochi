#lang racket
(require racket/list racket/string)

(define people (list (hash "name" "Alice" "age" 17 "status" "minor") (hash "name" "Bob" "age" 25 "status" "unknown") (hash "name" "Charlie" "age" 18 "status" "unknown") (hash "name" "Diana" "age" 16 "status" "minor")))
(set! people (for/list ([item people])
  (begin
    (when (>= (hash-ref item "age") 18)
