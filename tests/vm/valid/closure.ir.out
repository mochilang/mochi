func makeAdder (regs=3)
  // return fun(x: int): int => x + n
  Move         r1, r0
  MakeClosure  r2, fn2, 1, r1
  Return       r2

  // return fun(x: int): int => x + n
func fn2 (regs=3)
  // return fun(x: int): int => x + n
  Add          r2, r1, r0
  Return       r2
