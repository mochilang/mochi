let s: string = "mochi"
fun main() {
  print(_indexString(s, 1))
}
fun _indexString(s: string, i: int): string {
  let runes = s.runes.toList()
  if (i < 0) {
    i += runes.length
  }
  if (i < 0 || i >= runes.length) {
    throw RangeError('index out of range')
  }
  return String.fromCharCode(runes[i])
}
