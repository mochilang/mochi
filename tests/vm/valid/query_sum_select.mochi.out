fun _sum(v) {
  if hasattr(v, 'Items') {
  let v = v.Items
}
  if not isinstance(v, list) {
  raise Exception('sum() expects list or group')
}
  let s = 0.0
  for it in v {
  if it is None {
  continue
}
  if isinstance(it, (int, float)) {
  let s = s + float(it)
} else {
  raise Exception('sum() expects numbers')
}
}
  return s
}
