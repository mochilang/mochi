let people = [{"name": "Alice", "age": 30}, {"name": "Bob", "age": 15}, {"name": "Charlie", "age": 65}, {"name": "Diana", "age": 45}]
let adults = from person in people where person["age"] >= 18 select {"name": person["name"], "age": person["age"], "is_senior": person["age"] >= 60}
print("--- Adults ---")
for person in adults {
  print()
}
