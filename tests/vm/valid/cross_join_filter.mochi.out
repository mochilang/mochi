let nums = [1, 2, 3]
let letters = ["A", "B"]
let pairs = fun() {
    let _res = []
    for _, n in ipairs(nums) {
      for _, l in ipairs(letters) {
        if (n % 2 == 0) {
          _res[len(_res) + 1] = { "n": n, "l": l }
        }
      }
    }
    return _res
}()
print("--- Even pairs ---")
for _, p in ipairs(pairs) {
  print(p["n"], p["l"])
}
