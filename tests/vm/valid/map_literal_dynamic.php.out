<?php
$x = 3;
$y = 4;
$m = ["a" => $x, "b" => $y];
_print($m["a"], $m["b"]);

function _print(...$args) {
    $parts = [];
    foreach ($args as $a) {
        if (is_array($a) || is_object($a)) { $parts[] = json_encode($a); } else { $parts[] = strval($a); }
    }
    echo implode(' ', $parts), PHP_EOL;
}
