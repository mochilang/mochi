List<int> nums = [1, 2];

List<String> letters = ["A", "B"];

List<bool> bools = [true, false];

List<Map<String, dynamic>> combos = (() {
  var _res = [];
  for (var n in nums) {
    for (var l in letters) {
      for (var b in bools) {
        _res.add({"n": n, "l": l, "b": b});
      }
    }
  }
  return _res;
})();

void main() {
  print("--- Cross Join of three lists ---");
  for (var c in combos) {
    print([c.n.toString(), c.l.toString(), c.b.toString()].join(' '));
  }
}

