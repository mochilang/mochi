<?php
function mochi_boom($a, $b) {
	_print("boom");
	return true;
}

_print((false && mochi_boom(1, 2)));
_print((true || mochi_boom(1, 2)));

function _print(...$args) {
    $parts = [];
    foreach ($args as $a) {
        if (is_array($a) || is_object($a)) { $parts[] = json_encode($a); } else { $parts[] = strval($a); }
    }
    echo implode(' ', $parts), PHP_EOL;
}
