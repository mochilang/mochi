module Tree; end
type Leaf {
  include Tree
}
var Node = Struct.new(:left, :value, :right, keyword_init: true) do
include Tree
fun isLeaf(t) {
  (begin
  let _t0 = t
  _t0.is_a?(Leaf)) || false
  end)
}
print([isLeaf(Leaf.new)].join(" "))
print([isLeaf(Node.new(left: Leaf.new, value: 1, right: Leaf.new))].join(" "))
