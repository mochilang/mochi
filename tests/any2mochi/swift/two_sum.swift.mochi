fun _index(arr, i: int) {
    let idx = i
    let n = arr.count
    if idx < 0 { idx += n }
    if idx < 0 || idx >= n { fatalError("index out of range") }
    return arr[idx]
}
fun twoSum(nums, target: int) {
    let nums = nums
    let target = target
    let n = nums.count
    for i in 0..n - 1 {
      for j in i + 1..n - 1 {
        if _index(nums, i) + _index(nums, j) == target {
          return [i, j]
        }
      }
    }
    return [-1, -1]
}
fun main() {
    let result = twoSum([2, 7, 11, 15], 9)
    print(_index(result, 0))
    print(_index(result, 1))
}
main()
