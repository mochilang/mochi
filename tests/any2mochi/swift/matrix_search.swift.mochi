fun _index(arr, i: int) {
    let idx = i
    let n = arr.count
    if idx < 0 { idx += n }
    if idx < 0 || idx >= n { fatalError("index out of range") }
    return arr[idx]
}
fun searchMatrix(matrix, target: int): bool {
    let matrix = matrix
    let target = target
    let m = matrix.count
    if m == 0 {
      return false
    }
    let n = _index(matrix, 0).count
    let left = 0
    let right = m * n - 1
    while left <= right {
      let mid = left + (right - left) / 2
      let row = mid / n
      let col = mid % n
      let value = _index(_index(matrix, row), col)
      if value == target {
        return true
        } else if value < target {
        left = mid + 1
        } else {
        right = mid - 1
      }
    }
    return false
}
fun main() {
    print(searchMatrix([[1, 3, 5, 7], [10, 11, 16, 20], [23, 30, 34, 60]], 3))
    print(searchMatrix([[1, 3, 5, 7], [10, 11, 16, 20], [23, 30, 34, 60]], 13))
}
main()
