fun _sliceString(s: string, i: int, j: int): string {
    let start = i
    let end = j
    let chars = Array(s)
    let n = chars.count
    if start < 0 { start += n }
    if end < 0 { end += n }
    if start < 0 { start = 0 }
    if end > n { end = n }
    if end < start { end = start }
    return String(chars[start..<end])
}
fun main() {
    print(_sliceString("hello", 1, 4))
}
main()
