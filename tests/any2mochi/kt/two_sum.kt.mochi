fun twoSum(nums: list<int>, target: int): list<int> {
  let n = nums.size
  for i in 0 .. n {
    for j in (i + 1 {
      if nums[i] + nums[j]) == target {
        return listOf(i, j)
      }
    }
  }
  return listOf(-1, -1)
}
fun main() {
  let result = twoSum(listOf(2, 7, 11, 15), 9)
  print(result[0])
  print(result[1])
}
main()
