fun searchMatrix(matrix: list<list<int>>, target: int): bool {
  let m = matrix.size
  if m == 0 {
    return false
  }
  let n = matrix[0].size
  var left = 0
  var right = ((m * n) - 1)
  while left <= right {
    let mid = (left + (((right - left)) / 2))
    let row = (mid / n)
    let col = (mid % n)
    let value = matrix[row][col]
    if value == target {
      return true
      } else if ((value < target)) {
      left = (mid + 1)
      } else {
      right = (mid - 1)
    }
  }
  return false
}
fun main() {
  print(searchMatrix(listOf(listOf(1, 3, 5, 7), listOf(10, 11, 16, 20), listOf(23, 30, 34, 60)), 3))
  print(searchMatrix(listOf(listOf(1, 3, 5, 7), listOf(10, 11, 16, 20), listOf(23, 30, 34, 60)), 13))
}
main()
