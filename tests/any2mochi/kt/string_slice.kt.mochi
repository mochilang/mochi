fun main() {
  print(_sliceString("hello", 1, 4))
}
fun _sliceString(s, i, j) {
  var start = i
  var end = j
  let arr = s.toCharArray()
  let n = arr.size
  if start < 0) start += n {
    if end < 0) end += n {
      if start < 0) start = 0 {
        if end > n) end = n {
          if end < start) end = start {
            return String(arr.sliceArray(start until end))
          }
main()
