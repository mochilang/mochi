fun lambda_0() {
  n = 0
  for i in 1..size(vsrc) {
    it = vsrc(i)
    if (it.price < 150) {
      sort_key = it.price
      n = n + 1
      tmp(n) = it
      tmpKey(n) = sort_key
    }
  }
  for i in 1..n - 1 {
    min_idx = i
    for j in i + 1..n {
      if tmpKey(j) < tmpKey(min_idx) {
        min_idx = j
      }
    }
    if min_idx /= i {
      swap_key = tmpKey(i)
      tmpKey(i) = tmpKey(min_idx)
      tmpKey(min_idx) = swap_key
      swap_item = tmp(i)
      tmp(i) = tmp(min_idx)
      tmp(min_idx) = swap_item
    }
}
