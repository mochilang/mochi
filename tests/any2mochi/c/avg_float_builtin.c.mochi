type list_int {
  len: int
  *data: int
}

type list_float {
  len: int
  *data: float
}

fun _avg_float(v: list<float>): float {
  if (v.len == 0)
  return 0
  var sum = 0
  for (int i = 0; i < v.len; i++)
  sum = sum + v.data[i]
  return sum / v.len
}
list_float _t1 = list_float_create(3)
_t1.data[0] = 1.0
_t1.data[1] = 2.0
_t1.data[2] = 3.0
print(_avg_float(_t1))
