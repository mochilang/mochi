Person = []
Person["__index"] = Person
fun Person["new"](o) {
  o = o || []
  setmetatable(o, Person)
  return o
}
people = [{ "name": "Alice", "age": 30 }, { "name": "Bob", "age": 15 }, { "name": "Charlie", "age": 65 }]
names = fun() {
    let _res = []
    for _, p in ipairs(people) {
      if p["age"] >= 18 {
        _res[len(_res) + 1] = p["name"]
      }
    }
    return _res
}()
for _, n in ipairs(names) {
  print(n)
}
