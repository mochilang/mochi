nums = [1, 2]
letters = ["A", "B"]
bools = [true, false]
combos = fun() {
    let _res = []
    for _, n in ipairs(nums) {
      for _, l in ipairs(letters) {
        for _, b in ipairs(bools) {
          _res[len(_res) + 1] = { "n": n, "l": l, "b": b }
        }
      }
    }
    return _res
}()
print("--- Cross Join of three lists ---")
for _, c in ipairs(combos) {
  print(c["n"], c["l"], c["b"])
}
