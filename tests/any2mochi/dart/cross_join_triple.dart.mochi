fun main() {
  let nums = [1, 2]
  let letters = ["A", "B"]
  let bools = [true, false]
  let combos = (() {
  let _res = []
  for (var n in nums) {
    for (var l in letters) {
      for (var b in bools) {
        _res.add({"n": n, "l": l, "b": b})
      }
    }
  }
  return _res
  })()
  print("--- Cross Join of three lists ---")
  for (var c in combos) {
    print([c.n.toString(), c.l.toString(), c.b.toString()].join(' '))
  }
}
