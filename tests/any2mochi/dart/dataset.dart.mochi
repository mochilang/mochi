fun main() {
  List<Person> people = [Person(name: "Alice", age: 30), Person(name: "Bob", age: 15), Person(name: "Charlie", age: 65)]
  List<String> names = (() {
  let _res = []
  let _src = (people).where((p) => (p.age >= 18)).toList()
  for (var p in _src) {
    _res.add(p.name)
  }
  return _res
  })()
  for (var n in names) {
    print(n)
  }
}
