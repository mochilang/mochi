fun searchMatrix(matrix: any, target: any): any {
  dynamic m = matrix.length
  if ((m == 0)) {
    return false
  }
  dynamic n = matrix[0].length
  dynamic left = 0
  dynamic right = ((m * n) - 1)
  while ((left <= right)) {
    dynamic mid = (left + (((right - left)) ~/ 2))
    dynamic row = (mid ~/ n)
    dynamic col = (mid % n)
    dynamic value = matrix[row][col]
    if ((value == target)) {
      return true
      } else
      if ((value < target)) {
        left = (mid + 1)
        } else {
        right = (mid - 1)
      }
    }
    return false
}
fun main() {
  print(searchMatrix([[1, 3, 5, 7], [10, 11, 16, 20], [23, 30, 34, 60]], 3))
  print(searchMatrix([[1, 3, 5, 7], [10, 11, 16, 20], [23, 30, 34, 60]], 13))
}
