fun searchMatrix(matrix: any, target: any): any {
  let m = len(matrix)
  if ((m == 0)) {
    return false
  }
  let n = len(matrix[0])
  let left = 0
  let right = ((m * n) - 1)
  while ((left <= right)) {
    let mid = (left + (((right - left)) ~/ 2))
    let row = (mid ~/ n)
    let col = (mid % n)
    let value = matrix[row][col]
    if ((value == target)) {
      return true
      } else
      if ((value < target)) {
        left = (mid + 1)
        } else {
        right = (mid - 1)
      }
    }
    return false
}
fun main() {
  print(searchMatrix([[1, 3, 5, 7], [10, 11, 16, 20], [23, 30, 34, 60]], 3))
  print(searchMatrix([[1, 3, 5, 7], [10, 11, 16, 20], [23, 30, 34, 60]], 13))
}
