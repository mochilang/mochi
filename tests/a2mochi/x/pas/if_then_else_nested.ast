(program
  (var x (type int) (int 0))
  (var msg (type string) (string ""))
  (assign x (int 8))
  (assign msg
    (call IfThen
      (binary > (selector x) (int 10))
      (string big)
      (call IfThen
        (binary > (selector x) (int 5))
        (string medium)
        (string small)
      )
    )
  )
  (call print (selector msg))
)
