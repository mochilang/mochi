(program
  (let customers
    (list
      (map
        (entry (string id) (int 1))
        (entry (string name) (string Alice))
      )
      (map
        (entry (string id) (int 2))
        (entry (string name) (string Bob))
      )
    )
  )
  (let orders
    (list
      (map
        (entry (string id) (int 100))
        (entry (string customerId) (int 1))
      )
      (map
        (entry (string id) (int 101))
        (entry (string customerId) (int 2))
      )
    )
  )
  (let items
    (list
      (map
        (entry (string orderId) (int 100))
        (entry (string sku) (string a))
      )
      (map
        (entry (string orderId) (int 101))
        (entry (string sku) (string b))
      )
    )
  )
  (let result (list))
  (for o
    (in (selector orders))
    (block
      (for c
        (in (selector customers))
        (block
          (for i
            (in (selector items))
            (block
              (if
                (binary &&
                  (group
                    (binary ==
                      (index
                        (group
                          (cast
                            (selector o)
                            (type map (type string) (type any))
                          )
                        )
                        (string customerId)
                      )
                      (index
                        (group
                          (cast
                            (selector c)
                            (type map (type string) (type any))
                          )
                        )
                        (string id)
                      )
                    )
                  )
                  (group
                    (binary ==
                      (index
                        (group
                          (cast
                            (selector o)
                            (type map (type string) (type any))
                          )
                        )
                        (string id)
                      )
                      (index
                        (group
                          (cast
                            (selector i)
                            (type map (type string) (type any))
                          )
                        )
                        (string orderId)
                      )
                    )
                  )
                )
                (block)
              )
            )
          )
        )
      )
    )
  )
  (call print (string "--- Multi Join ---"))
  (for r
    (in (selector result))
    (block
      (call print
        (index
          (group
            (cast
              (selector r)
              (type map (type string) (type any))
            )
          )
          (string name)
        )
        (string " bought item")
        (index
          (group
            (cast
              (selector r)
              (type map (type string) (type any))
            )
          )
          (string sku)
        )
      )
    )
  )
)
