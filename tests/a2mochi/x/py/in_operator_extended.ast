(program
  (var xs
    (list (int 1) (int 2) (int 3))
  )
  (var ys
    (query x
      (source (selector xs))
      (where
        (binary ==
          (binary % (selector x) (int 2))
          (int 1)
        )
      )
      (select (selector x))
    )
  )
  (call print
    (binary in (int 1) (selector ys))
  )
  (call print
    (binary in (int 2) (selector ys))
  )
  (var m
    (map
      (entry (string a) (int 1))
    )
  )
  (call print
    (binary in (string a) (selector m))
  )
  (call print
    (binary in (string b) (selector m))
  )
  (var s (string hello))
  (call print
    (binary in (string ell) (selector s))
  )
  (call print
    (binary in (string foo) (selector s))
  )
)
