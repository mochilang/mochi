  "Src": "// Generated by Mochi 0.10.35 on 2025-07-22 10:54 +0700\nusing System;\nusing math = System.Math;\n\nclass Program {\n    static double r = 3;\n    static double area = (Math.PI * Math.Pow(r, 2));\n    static double root = Math.Sqrt(49);\n    static double sin45 = Math.Sin((Math.PI / 4));\n    static double log_e = Math.Log(Math.E);\n    static void Main() {\n        Console.WriteLine(string.Join(\" \", new object[]{\"Circle area with r =\", r, \"=\u003e\", area}));\n        Console.WriteLine(string.Join(\" \", new object[]{\"Square root of 49:\", root}));\n        Console.WriteLine(string.Join(\" \", new object[]{\"sin(π/4):\", sin45}));\n        Console.WriteLine(string.Join(\" \", new object[]{\"log(e):\", log_e}));\n    }\n}\n"
