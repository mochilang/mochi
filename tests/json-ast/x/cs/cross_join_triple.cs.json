  "Src": "// Generated by Mochi 0.10.33 on 2025-07-21 18:06 +0700\nusing System;\nusing System.Collections.Generic;\nusing System.Linq;\n\nstruct NResult {\n    public int n;\n    public string l;\n    public bool b;\n    public override string ToString() =\u003e $\"{{\\\"n\\\": {n}, \\\"l\\\": \\\"{l}\\\", \\\"b\\\": {b}}}\";\n}\nclass Program {\n    static int[] nums = new int[]{1, 2};\n    static string[] letters = new string[]{\"A\", \"B\"};\n    static bool[] bools = new bool[]{true, false};\n    static NResult[] combos = (from n in nums from l in letters from b in bools select new NResult{n = n, l = l, b = b}).ToArray();\n    static void Main() {\n        Console.WriteLine(\"--- Cross Join of three lists ---\");\n        foreach (var c in combos) {\n    Console.WriteLine(string.Join(\" \", new object[]{c.n, c.l, (c.b ? \"true\" : \"false\")}).TrimEnd());\n}\n    }\n}\n"
