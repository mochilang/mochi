  "Src": "// Generated by Mochi 0.10.34 on 2025-07-22 06:00 +0700\nusing System;\nusing System.Collections.Generic;\n\nstruct Customer {\n    public int id;\n    public string name;\n    public override string ToString() =\u003e $\"{{'id': {id}, 'name': '{name}'}}\";\n}\nstruct Order {\n    public int id;\n    public int customerId;\n    public int total;\n    public override string ToString() =\u003e $\"{{'id': {id}, 'customerId': {customerId}, 'total': {total}}}\";\n}\nclass Program {\n    static Customer[] customers = new Customer[]{new Customer{id = 1, name = \"Alice\"}, new Customer{id = 2, name = \"Bob\"}, new Customer{id = 3, name = \"Charlie\"}, new Customer{id = 4, name = \"Diana\"}};\n    static Order[] orders = new Order[]{new Order{id = 100, customerId = 1, total = 250}, new Order{id = 101, customerId = 2, total = 125}, new Order{id = 102, customerId = 1, total = 300}, new Order{id = 103, customerId = 5, total = 80}};\n    static Dictionary\u003cstring, object\u003e[] result = (() =\u003e { var _res = new List\u003cDictionary\u003cstring, object\u003e\u003e(); foreach (var o in orders) { bool _matched = false; foreach (var c in customers) { if (!((o.customerId == c.id))) continue; _matched = true; _res.Add(new Dictionary\u003cstring, object\u003e{{\"order\", o}, {\"customer\", c}}); } if (!_matched) { var c = default(Customer); _res.Add(new Dictionary\u003cstring, object\u003e{{\"order\", o}, {\"customer\", c}}); } } foreach (var c in customers) { bool _exists = false; foreach (var o in orders) { if ((o.customerId == c.id)) { _exists = true; break; } } if (!_exists) { var o = default(Order); _res.Add(new Dictionary\u003cstring, object\u003e{{\"order\", o}, {\"customer\", c}}); } } return _res.ToArray(); })();\n    static void Main() {\n        Console.WriteLine(\"--- Outer Join using syntax ---\");\n        foreach (var row in result) {\n    if (row.order) {\n    if (row.customer) {\n    Console.WriteLine((\"[\" + (string.Join(\", \", new object[]{\"Order\", row.order.id, \"by\", row.customer.name, \"- $\", row.order.total}) + \"]\")));\n} else {\n    Console.WriteLine((\"[\" + (string.Join(\", \", new object[]{\"Order\", row.order.id, \"by\", \"Unknown\", \"- $\", row.order.total}) + \"]\")));\n}\n} else {\n    Console.WriteLine((\"[\" + (string.Join(\", \", new object[]{\"Customer\", row.customer.name, \"has no orders\"}) + \"]\")));\n}\n}\n    }\n}\n"
