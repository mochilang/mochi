object Main {
	def searchMatrix(matrix: List[List[Int]], target: Int): Boolean = {
		val m = matrix.length
		if ((m == 0)) {
			return false
		}
		val n = matrix(0).length
		var left = 0
		var right = ((m * n) - 1)
		while ((left <= right)) {
			val mid = ((left + ((right - left))) / 2)
			val row = (mid / n)
			val col = (mid % n)
			val value = matrix(row)(col)
			if ((value == target)) {
				return true
			} else 			if ((value < target)) {
				left = (mid + 1)
			} else {
				right = (mid - 1)
			}
		}
		return false
	}
	
	def main(args: Array[String]): Unit = {
		println(searchMatrix(List(List(1, 3, 5, 7), List(10, 11, 16, 20), List(23, 30, 34, 60)), 3))
		println(searchMatrix(List(List(1, 3, 5, 7), List(10, 11, 16, 20), List(23, 30, 34, 60)), 13))
	}
}

