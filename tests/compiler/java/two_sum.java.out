public class Main {
    static Object[] twoSum(Object[] nums, int target) {
        int n = nums.length;
        for (int i = 0; i < n; i++) {
            for (int j = (i + 1); j < n; j++) {
                if (((nums[i] + nums[j]) == target)) {
                    return new int[]{i, j};
                }
            }
        }
        return new int[]{(-1), (-1)};
    }

    public static void main(String[] args) {
        Object[] result = twoSum(new int[]{2, 7, 11, 15}, 9);
        System.out.println(result[0]);
        System.out.println(result[1]);
    }
}
