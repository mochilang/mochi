public class Main {
    // fun fib(n: int): int {
    static int fib(int n) {
        //   if n <= 1 {
        if ((n <= 1)) {
            //     return n
            return n;
        }
        //   return fib(n - 1) + fib(n - 2)
        return (fib((n - 1)) + fib((n - 2)));
    }
    
    public static void main(String[] args) {
        // print(fib(0))
        System.out.println(fib(0));
        // print(fib(1))
        System.out.println(fib(1));
        // print(fib(6))
        System.out.println(fib(6));
    }
}
