public class Main {
    static int factorial(int n) {
        if ((n <= 1)) {
            return 1;
        }
        return (n * factorial((n - 1)));
    }

    public static void main(String[] args) {
        System.out.println(factorial(0));
        System.out.println(factorial(1));
        System.out.println(factorial(5));
    }
}
