public class Main {
    // fun searchMatrix(matrix: list<list<int>>, target: int): bool {
    static boolean searchMatrix(Object[] matrix, int target) {
        //   let m = len(matrix)
        int m = matrix.length;
        //   if m == 0 {
        if ((m == 0)) {
            //     return false
            return false;
        }
        //   let n = len(matrix[0])
        int n = matrix[0].length;
        //   var left = 0
        int left = 0;
        //   var right = m * n - 1
        int right = ((m * n) - 1);
        //   while left <= right {
        while ((left <= right)) {
            //     let mid = left + (right - left) / 2
            int mid = (left + ((right - left) / 2));
            //     let row = mid / n
            int row = (mid / n);
            //     let col = mid % n
            int col = (mid % n);
            //     let value = matrix[row][col]
            int value = matrix[row][col];
            //     if value == target {
            if ((value == target)) {
                //       return true
                return true;
            } else             if ((value < target)) {
                //       left = mid + 1
                left = (mid + 1);
            } else {
                //       right = mid - 1
                right = (mid - 1);
            }
        }
        //   return false
        return false;
    }
    
    public static void main(String[] args) {
        // print(searchMatrix([[1,3,5,7],[10,11,16,20],[23,30,34,60]], 3))
        System.out.println(searchMatrix(new Object[][]{new int[]{1, 3, 5, 7}, new int[]{10, 11, 16, 20}, new int[]{23, 30, 34, 60}}, 3));
        // print(searchMatrix([[1,3,5,7],[10,11,16,20],[23,30,34,60]], 13))
        System.out.println(searchMatrix(new Object[][]{new int[]{1, 3, 5, 7}, new int[]{10, 11, 16, 20}, new int[]{23, 30, 34, 60}}, 13));
    }
}
