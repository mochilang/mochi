class Main {
	static boolean searchMatrix(int[][] matrix, int target) {
		var m = matrix.length;
		if ((m == 0)) {
			return false;
		}
		var n = matrix[0].length;
		var left = 0;
		var right = ((m * n) - 1);
		while ((left <= right)) {
			var mid = ((left + (right - left)) / 2);
			var row = (mid / n);
			var col = (mid % n);
			var value = matrix[row][col];
			if ((value == target)) {
				return true;
			} else 			if ((value < target)) {
				left = (mid + 1);
			} else {
				right = (mid - 1);
			}
		}
		return false;
	}
	
	public static void main(String[] args) {
		System.out.println(searchMatrix(new int[][]{new int[]{1, 3, 5, 7}, new int[]{10, 11, 16, 20}, new int[]{23, 30, 34, 60}}, 3));
		System.out.println(searchMatrix(new int[][]{new int[]{1, 3, 5, 7}, new int[]{10, 11, 16, 20}, new int[]{23, 30, 34, 60}}, 13));
	}
}
