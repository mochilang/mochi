public class Main {
  public static void main(String[] args) {
    System.out.println(_sliceString("hello", 1, 4));
  }

  static String _sliceString(String s, int i, int j) {
    int start = i;
    int end = j;
    int n = s.length();
    if (start < 0) start += n;
    if (end < 0) end += n;
    if (start < 0) start = 0;
    if (end > n) end = n;
    if (end < start) end = start;
    return s.substring(start, end);
  }
}
