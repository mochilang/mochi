/*
var nums = [1, 2]
nums[1] = 3
print(nums[1])
*/
#include <stdio.h>
#include <stdlib.h>

typedef struct {
  int len;
  int *data;
} list_int;
static list_int list_int_create(int len) {
  list_int l;
  l.len = len;
  l.data = (int *)malloc(sizeof(int) * len);
  return l;
}
int main() {
  /* var nums = [1, 2] */
  list_int _t1 = list_int_create(2);
  _t1.data[0] = 1;
  _t1.data[1] = 2;
  list_int nums = _t1;
  /* nums[1] = 3 */
  nums.data[1] = 3;
  /* print(nums[1]) */
  printf("%d\n", nums.data[1]);
  return 0;
}
