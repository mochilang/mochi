#include <bits/stdc++.h>
using namespace std;

// fun makeAdder(n: int): fun(int): int {
auto makeAdder(int n){
    //   return fun(x: int): int => x + n
    return [=](int x) { return x + n; };
}

int main() {
    // let add10 = makeAdder(10)
    auto add10 = makeAdder(10);
    // print(add10(7))  // 17
    std::cout << (add10(7)) << std::endl;
    return 0;
}
