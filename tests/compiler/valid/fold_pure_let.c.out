#include <stdio.h>
#include <stdlib.h>

typedef struct {
  int len;
  int *data;
} list_int;
static list_int list_int_create(int len) {
  list_int l;
  l.len = len;
  l.data = (int *)malloc(sizeof(int) * len);
  return l;
}
/* fun sumN(n: int): int { */
int sumN(int n) {
  /* return n * (n + 1) / 2 */
  return ((n * ((n + 1))) / 2);
}

int main() {
  /* let n = 10 */
  int n = 10;
  /* print(sumN(n)) */
  printf("%d\n", sumN(n));
  /* print(n) */
  printf("%d\n", n);
  return 0;
}
