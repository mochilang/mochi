#include <stdio.h>
#include <stdlib.h>

typedef struct { int len; int *data; } list_int;

static list_int list_int_create(int len) {
	list_int l;
	l.len = len;
	l.data = (int*)malloc(sizeof(int)*len);
	return l;
}


int main() {
	int scores = 0;
	scores.data["b"] = 2;
	printf("%d\n", scores.data["b"]);
	return 0;
}
