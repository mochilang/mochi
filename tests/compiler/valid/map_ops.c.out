#include <stdio.h>
#include <stdlib.h>

typedef struct { int len; int *data; } list_int;

static list_int list_int_create(int len) {
	list_int l;
	l.len = len;
	l.data = (int*)malloc(sizeof(int)*len);
	return l;
}


int main() {
	int m = 0;
	m.data[1] = 10;
	m.data[2] = 20;
	if ((1 in m)) {
		printf("%d\n", m.data[1]);
	}
	printf("%d\n", m.data[2]);
	return 0;
}
