#include <stdio.h>
#include <stdlib.h>

typedef struct { int len; int *data; } list_int;

static list_int list_int_create(int len) {
	list_int l;
	l.len = len;
	l.data = (int*)malloc(sizeof(int)*len);
	return l;
}

static int _count(list_int v) {
	return v.len;
}

static double _avg(list_int v) {
	if (v.len == 0) return 0;
	double sum = 0;
	for (int i = 0; i < v.len; i++) {
		sum += v.data[i];
	}
	return sum / v.len;
}

int main() {
	list_int _t1 = list_int_create(3);
	_t1.data[0] = 10;
	_t1.data[1] = 20;
	_t1.data[2] = 30;
	list_int xs = _t1;
	printf("%d\n", xs.data[1]);
	return 0;
}
