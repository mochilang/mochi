// Generated by Mochi TypeScript compiler

type Customer = {
	id: number;
	name: string;
}

type Order = {
	id: number;
	customerId: number;
	total: number;
}

type PairInfo = {
	orderId: number;
	customerName: string;
	total: number;
}

let customers: Array<Customer> = [{id: 1, name: "Alice"}, {id: 2, name: "Bob"}, {id: 3, name: "Charlie"}]

let orders: Array<Order> = [{id: 100, customerId: 1, total: 250}, {id: 101, customerId: 2, total: 125}, {id: 102, customerId: 1, total: 300}, {id: 103, customerId: 4, total: 80}]

let result: Array<PairInfo> = (() => {
	const _src = orders;
	const _res = [];
	for (const o of _src) {
		for (const c of customers) {
			if (!((o.customerId == c.id))) { continue }
			_res.push({orderId: o.id, customerName: c.name, total: o.total})
		}
	}
	return _res;
})()

function main(): void {
	console.log("--- Orders with customer info ---")
	for (const entry of result) {
		console.log("Order", entry.orderId, "by", entry.customerName, "- $", entry.total)
	}
}
main()
