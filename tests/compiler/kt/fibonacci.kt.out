// fun fib(n: int): int {
fun fib(n: Int) : Int {
        if (n <= 1) {
                return n
        }
        return fib(n - 1) + fib(n - 2)
}

fun main() {
        // print(fib(0))
        println(fib(0))
        // print(fib(1))
        println(fib(1))
        // print(fib(6))
        println(fib(6))
}
