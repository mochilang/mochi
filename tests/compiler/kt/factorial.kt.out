// fun factorial(n: int): int {
fun factorial(n: Int) : Int {
        if (n <= 1) {
                return 1
        }
        return n * factorial(n - 1)
}

fun main() {
        // print(factorial(0))
        println(factorial(0))
        // print(factorial(1))
        println(factorial(1))
        // print(factorial(5))
        println(factorial(5))
}
