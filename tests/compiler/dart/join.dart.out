class Customer {
	dynamic id;
	dynamic name;
	Customer({this.id, this.name});
}

class Order {
	dynamic id;
	dynamic customerId;
	dynamic total;
	Order({this.id, this.customerId, this.total});
}

class PairInfo {
	dynamic orderId;
	dynamic customerName;
	dynamic total;
	PairInfo({this.orderId, this.customerName, this.total});
}

void main() {
	var customers = [Customer(id: 1, name: "Alice"), Customer(id: 2, name: "Bob"), Customer(id: 3, name: "Charlie")];
	var orders = [Order(id: 100, customerId: 1, total: 250), Order(id: 101, customerId: 2, total: 125), Order(id: 102, customerId: 1, total: 300), Order(id: 103, customerId: 4, total: 80)];
	var result = (() {
	var _res = [];
	for (var o in orders) {
		for (var c in customers) {
			if (!((o.customerId == c.id))) {
				continue;
			}
			_res.add(PairInfo(orderId: o.id, customerName: c.name, total: o.total));
		}
	}
	return _res;
})();
	print("--- Orders with customer info ---");
	for (var entry in result) {
		print(["Order".toString(), entry.orderId.toString(), "by".toString(), entry.customerName.toString(), "- $".toString(), entry.total.toString()].join(' '));
	}
}
