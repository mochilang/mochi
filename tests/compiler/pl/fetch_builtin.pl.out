fetch_data(URL, _Opts, Data) :-
    atom_string(U, URL),
    ( sub_atom(U, 0, 7, _, 'file://') ->
        sub_atom(U, 7, _, 0, Path),
        read_file_to_string(Path, Text, [])
    ;   setup_call_cleanup(http_open(U, S, []), read_string(S, _, Text), close(S))
    ),
    atom_json_dict(Text, Data, []).



	main :-
