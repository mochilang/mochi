#include <stdio.h>
#include <stdlib.h>

typedef struct { int len; int *data; } list_int;
static list_int list_int_create(int len) {
    list_int l;
    l.len = len;
    l.data = (int*)malloc(sizeof(int)*len);
    return l;
}
typedef struct { int len; double *data; } list_float;
static list_float list_float_create(int len) {
    list_float l;
    l.len = len;
    l.data = (double*)malloc(sizeof(double)*len);
    return l;
}
static double _avg_float(list_float v) {
    if (v.len == 0) return 0;
    double sum = 0;
    for (int i = 0; i < v.len; i++) sum += v.data[i];
    return sum / v.len;
}
int main() {
	list_float _t1 = list_float_create(3);
	_t1.data[0] = 1.0;
	_t1.data[1] = 2.0;
	_t1.data[2] = 3.0;
	printf("%g\n", _avg_float(_t1));
	return 0;
}
