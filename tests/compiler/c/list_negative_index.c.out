#include <stdio.h>
#include <stdlib.h>

typedef struct { int len; int *data; } list_int;

static list_int list_int_create(int len) {
        list_int l;
        l.len = len;
        l.data = (int*)malloc(sizeof(int)*len);
        return l;
}

static int _index_list_int(list_int v, int i) {
        if (i < 0) i += v.len;
        if (i < 0 || i >= v.len) { fprintf(stderr, "index out of range\n"); exit(1); }
        return v.data[i];
}

int main() {
        list_int _t1 = list_int_create(3);
        _t1.data[0] = 1;
        _t1.data[1] = 2;
        _t1.data[2] = 3;
        list_int xs = _t1;
        int _t2 = _index_list_int(xs, (-1));
        printf("%d\n", _t2);
        return 0;
}
