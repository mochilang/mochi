#include <stdio.h>
#include <stdlib.h>
#include <string.h>

typedef struct { int len; int *data; } list_int;

static list_int list_int_create(int len) {
	list_int l;
	l.len = len;
	l.data = (int*)malloc(sizeof(int)*len);
	return l;
}


static char* _input() {
	char buf[1024];
	if (!fgets(buf, sizeof(buf), stdin)) return strdup("");
	size_t len = strlen(buf);
	if (len > 0 && buf[len-1] == '\n') buf[len-1] = '\0';
	return strdup(buf);
}

int main() {
	printf("%s\n", "Enter first input:");
	char* input1 = _input();
	printf("%s\n", "Enter second input:");
	char* input2 = _input();
	printf("%s ", "You entered:");
	printf("%s ", input1);
	printf("%s ", ",");
	printf("%s\n", input2);
	return 0;
}
