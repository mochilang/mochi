#include <stdio.h>
#include <stdlib.h>
#include <string.h>

typedef struct { int len; int *data; } list_int;
static list_int list_int_create(int len) {
    list_int l;
    l.len = len;
    l.data = (int*)malloc(sizeof(int)*len);
    return l;
}

int inc(int x){
        return (x + 1);
}

int apply(int (*f)(int), int x){
        return f(x);
}

int _lambda0(int y){
        return (y * 2);
}

int main() {
        printf("%d\n", apply(inc, 5));
        printf("%d\n", apply(_lambda0, 7));
        return 0;
}
