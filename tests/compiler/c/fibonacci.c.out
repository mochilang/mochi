#include <stdio.h>
#include <stdlib.h>

typedef struct { int len; int *data; } list_int;

static list_int list_int_create(int len) {
	list_int l;
	l.len = len;
	l.data = (int*)malloc(sizeof(int)*len);
	return l;
}


int fib(int n){
	if ((n <= 1)) {
		return n;
	}
	return (fib((n - 1)) + fib((n - 2)));
}

int main() {
	printf("%d\n", fib(0));
	printf("%d\n", fib(1));
	printf("%d\n", fib(6));
	return 0;
}
