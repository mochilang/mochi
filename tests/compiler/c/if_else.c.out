#include <stdio.h>
#include <stdlib.h>

typedef struct {
  int len;
  int *data;
} list_int;
static list_int list_int_create(int len) {
  list_int l;
  l.len = len;
  l.data = (int *)malloc(sizeof(int) * len);
  return l;
}
/* fun foo(n: int): int { */
int foo(int n) {
  /* if n < 0 { */
  if ((n < 0)) {
    /* return -1 */
    return (-1);
  } else if ((n == 0)) {
    /* return 0 */
    return 0;
  } else {
    /* return 1 */
    return 1;
  }
}

int main() {
  /* print(foo(-2)) */
  printf("%d\n", foo((-2)));
  /* print(foo(0)) */
  printf("%d\n", foo(0));
  /* print(foo(3)) */
  printf("%d\n", foo(3));
  return 0;
}
