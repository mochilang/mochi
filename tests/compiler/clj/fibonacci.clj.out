(ns main)

(defn fib [n]
  (try
    (when (<= n 1)
      (throw (ex-info "return" {:value n}))
    )
    (throw (ex-info "return" {:value (+ (fib (- n 1)) (fib (- n 2)))}))
  (catch clojure.lang.ExceptionInfo e
    (if (= (.getMessage e) "return")
      (:value (ex-data e))
    (throw e)))
  )
)

(defn -main []
  (println (fib 0))
  (println (fib 1))
  (println (fib 6))
)

(-main)
