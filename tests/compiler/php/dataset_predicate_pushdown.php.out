<?php
$xs = [1, 2, 3];
$ys = [10, 20];
$res = (function() use ($xs, $ys) {
	$res = [];
	foreach ((is_string($xs) ? str_split($xs) : $xs) as $x) {
		if (!(($x > 1))) continue;
		foreach ((is_string($ys) ? str_split($ys) : $ys) as $y) {
			$res[] = ($x * $y);
		}
	}
	return $res;
})();
foreach ((is_string($res) ? str_split($res) : $res) as $r) {
	echo $r, PHP_EOL;
}
