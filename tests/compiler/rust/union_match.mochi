type Tree =
  Leaf
  | Node(left: Tree, value: int, right: Tree)
fun isLeaf(t) {
  return (|| {
    match t {
        Tree::Leaf => { true },
        _ => { false },
    }
})()
}
fun main() {
  print(isLeaf(Tree::Leaf {  }))
  print(isLeaf(Tree::Node { left: Box::new(Tree::Leaf {  }), value: 1, right: Box::new(Tree::Leaf {  }) }))
}
