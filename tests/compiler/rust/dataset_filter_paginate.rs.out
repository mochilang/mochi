fn main() {
    let mut nums = vec![0, 1, 2, 3, 4, 5];
    let mut res = {
    let mut _res = Vec::new();
    for n in nums {
        if n > 1 {
            _res.push(n);
        }
    }
    let mut skip = 1 as usize;
    if skip < _res.len() { _res = _res[skip..].to_vec(); } else { _res = Vec::new(); }
    let take = 2 as usize;
    if take < _res.len() { _res.truncate(take); }
    _res
};
    for v in res {
        println!("{}", v);
    }
}

