#[derive(Clone, Debug)]
enum Tree {
    Leaf,
    Node {
        left: Box<Tree>,
        value: i32,
        right: Box<Tree>,
    },
}

fn isLeaf(t: Tree) -> bool {
    return (|| {
    match t {
        Tree::Leaf => { true },
        _ => { false },
    }
})();
}

fn main() {
    println!("{}", isLeaf(Tree::Leaf {  }));
    println!("{}", isLeaf(Tree::Node { left: Box::new(Tree::Leaf {  }), value: 1, right: Box::new(Tree::Leaf {  }) }));
}
