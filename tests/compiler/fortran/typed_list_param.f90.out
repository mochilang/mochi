program main
   implicit none
   print *, first((/'hello', 'world'/))
contains
   function first(xs) result(res)
      implicit none
      character(:), allocatable :: res
      character(len=*), intent(in) :: xs(:)
      res = xs(modulo(0_8, len(xs)) + 1:modulo(0_8, len(xs)) + 1)
      return
   end function first

end program main
