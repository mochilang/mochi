func fromList(nums []int) Tree {
	// line 8
	var helper func(int, int) Tree
	helper = func(lo int, hi int) Tree {
		if lo >= hi {
			return Leaf{}
		}
		var mid int = ((lo + hi) / 2)
		return Node{Left: helper(lo, mid), Value: nums[mid], Right: helper((mid + 1), hi)}
	}
	return helper(0, len(nums))
}

