package main

func expect(cond bool) {
	if !cond { panic("expect failed") }
}

func shortestDistance(grid [][]int) int {
	var rows int = len(grid)
	if (rows == 0) {
		return -1
	}
	var cols int = len(grid[0])
	var dist [][]int = [][]int{}
	var reach [][]int = [][]int{}
	var r int = 0
	for {
		if !((r < rows)) {
			break
		}
		var distRow []int = []int{}
		var reachRow []int = []int{}
		var c int = 0
		for {
			if !((c < cols)) {
				break
			}
			distRow = append(append([]int{}, distRow...), []int{0}...)
			reachRow = append(append([]int{}, reachRow...), []int{0}...)
			c = (c + 1)
		}
		dist = append(append([][]int{}, dist...), [][]int{distRow}...)
		reach = append(append([][]int{}, reach...), [][]int{reachRow}...)
		r = (r + 1)
	}
	var dirs [][]int = [][]int{[]int{1, 0}, []int{-1, 0}, []int{0, 1}, []int{0, -1}}
	var total int = 0
	r = 0
	for {
		if !((r < rows)) {
			break
		}
		var c int = 0
		for {
			if !((c < cols)) {
				break
			}
			if (grid[r][c] == 1) {
				total = (total + 1)
				var queue [][]int = [][]int{[]int{r, c}}
				var visited [][]bool = [][]bool{}
				var vr int = 0
				for {
					if !((vr < rows)) {
						break
					}
					var row []bool = []bool{}
					var vc int = 0
					for {
						if !((vc < cols)) {
							break
						}
						row = append(append([]bool{}, row...), []bool{false}...)
						vc = (vc + 1)
					}
					visited = append(append([][]bool{}, visited...), [][]bool{row}...)
					vr = (vr + 1)
				}
				visited[r][c] = true
				var idx int = 0
				var level int = 0
				for {
					if !((idx < len(queue))) {
						break
					}
					var size int = (len(queue) - idx)
					var i int = 0
					for {
						if !((i < size)) {
							break
						}
						var pos []int = queue[idx]
						idx = (idx + 1)
						var pr int = pos[0]
						var pc int = pos[1]
						dist[pr][pc] = (dist[pr][pc] + level)
						reach[pr][pc] = (reach[pr][pc] + 1)
						var d int = 0
						for {
							if !((d < len(dirs))) {
								break
							}
							var nr int = (pr + dirs[d][0])
							var nc int = (pc + dirs[d][1])
							if ((((nr >= 0) && (nr < rows)) && (nc >= 0)) && (nc < cols)) {
								if (grid[nr][nc] == 0) {
									if !(visited[nr][nc]) {
										visited[nr][nc] = true
										queue = append(append([][]int{}, queue...), [][]int{[]int{nr, nc}}...)
									}
								}
							}
							d = (d + 1)
						}
						i = (i + 1)
					}
					level = (level + 1)
				}
			}
			c = (c + 1)
		}
		r = (r + 1)
	}
	var best int = 2147483647
	r = 0
	for {
		if !((r < rows)) {
			break
		}
		var c int = 0
		for {
			if !((c < cols)) {
				break
			}
			if (grid[r][c] == 0) {
				if (reach[r][c] == total) {
					if (dist[r][c] < best) {
						best = dist[r][c]
					}
				}
			}
			c = (c + 1)
		}
		r = (r + 1)
	}
	if (best == 2147483647) {
		return -1
	}
	return best
}

func example_1() {
	expect((shortestDistance(grid1) == 7))
}

func simple() {
	expect((shortestDistance(grid2) == 1))
}

func empty() {
	expect((shortestDistance(grid3) == (-1)))
}

var grid1 [][]int = [][]int{[]int{1, 0, 2, 0, 1}, []int{0, 0, 0, 0, 0}, []int{0, 0, 1, 0, 0}}
var grid2 [][]int = [][]int{[]int{1, 0}, []int{0, 0}}
var grid3 [][]int = [][]int{}
func main() {
	example_1()
	simple()
	empty()
}
