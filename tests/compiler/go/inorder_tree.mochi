type Tree =
  Leaf
  | Node(left: Tree, value: int, right: Tree)

fun inorder(t: Tree): list<int> {
  return match t {
    Leaf => [] as list<int>
    Node(l, v, r) => inorder(l) + [v] + inorder(r)
  }
}

print(inorder(Node { left: Node { left: Leaf {}, value: 1, right: Leaf {} }, value: 2, right: Leaf {} }))
