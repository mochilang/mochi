package main

import (
	"encoding/json"
	"fmt"
)

type Foo interface { isFoo() }
type Empty struct {
}
func (Empty) isFoo() {}
type Node struct {
	Child Foo `json:"child"`
}
func (Node) isFoo() {}

func listit() []Foo {
	return _convSlice[Empty,Foo]([]Empty{Empty{}})
}

func main() {
	fmt.Println(len(listit()))
}

func _convSlice[T any, U any](s []T) []U {
    out := make([]U, len(s))
    for i, v := range s {
        if cv, ok := any(v).(U); ok {
            out[i] = cv
        } else {
            data, err := json.Marshal(v)
            if err != nil { panic(err) }
            var u U
            if err := json.Unmarshal(data, &u); err != nil { panic(err) }
            out[i] = u
        }
    }
    return out
}
