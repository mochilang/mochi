		long[] nums = new long[] { 1L, 2L };
		string[] letters = new string[] { "A", "B" };
		bool[] bools = new bool[] { true, false };
		var combos = new Func<List<Dictionary<string, dynamic>>>(() => {
	var _res = new List<Dictionary<string, dynamic>>();
	foreach (var n in nums) {
