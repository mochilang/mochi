function __print(...)
	local args = {...}
	for i, a in ipairs(args) do
		if i > 1 then io.write(' ') end
		io.write(tostring(a))
	end
	io.write('\n')
end

function __iter(obj)
	if type(obj) == 'table' then
		if obj[1] ~= nil or #obj > 0 then
			local i = 0
			local n = #obj
			return function()
				i = i + 1
				if i <= n then return i, obj[i] end
			end
		else
			return pairs(obj)
		end
	elseif type(obj) == 'string' then
		local i = 0
		local n = #obj
		return function()
			i = i + 1
			if i <= n then return i, string.sub(obj, i, i) end
		end
	else
		return function() return nil end
	end
end

local nums = {1, 2}
local letters = {"A", "B"}
local bools = {true, false}
local combos = (function()
	local _res = {}
	for _, n in ipairs(nums) do
		for _, l in ipairs(letters) do
			for _, b in ipairs(bools) do
				table.insert(_res, {__map=true, [n]=n, [l]=l, [b]=b})
			end
		end
	end
	return _res
end)()
__print("--- Cross Join of three lists ---")
for _, c in __iter(combos) do
	__print(c.n, c.l, c.b)
	::__continue0::
end
