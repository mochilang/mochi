function __print(...)
    local args = {...}
    for i, a in ipairs(args) do
        if i > 1 then io.write(' ') end
        io.write(tostring(a))
    end
    io.write('\n')
end
Todo = {}
Todo.__index = Todo
function Todo.new(o)
	o = o or {}
	setmetatable(o, Todo)
	return o
end

todo = Todo.new({["title"]="hi"})
__print(todo.title)
