function __add(a, b)
    if type(a) == "table" and type(b) == "table" then
        local out = {}
        for i = 1, #a do
            out[#out + 1] = a[i]
        end
        for i = 1, #b do
            out[#out + 1] = b[i]
        end
        return out
    elseif type(a) == "string" or type(b) == "string" then
        return tostring(a) .. tostring(b)
    else
        return a + b
    end
end
function __print(...)
    local args = {...}
    for i, a in ipairs(args) do
        if i > 1 then
            io.write(" ")
        end
        io.write(tostring(a))
    end
    io.write("\n")
end
function makeAdder(n)
    return function(x)
        return __add(x, n)
    end
end

add10 = makeAdder(10)
__print(add10(7))
