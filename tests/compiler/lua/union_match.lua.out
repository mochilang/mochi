function __print(...)
	local args = {...}
	for i, a in ipairs(args) do
		if i > 1 then io.write(' ') end
		io.write(tostring(a))
	end
	io.write('\n')
end

function isLeaf(t)
	return (function()
	local _t0 = t
	if _t0.__name == "Leaf" then return true end
	return false
end)()
end

__print(isLeaf({__name="Leaf"}))
__print(isLeaf({__name="Node", left={__name="Leaf"}, value=1, right={__name="Leaf"}}))
