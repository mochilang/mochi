function __print(...)
	local args = {...}
	for i, a in ipairs(args) do
		if i > 1 then io.write(' ') end
		io.write(tostring(a))
	end
	io.write('\n')
end

function depth(t)
	return (function()
	local _t0 = t
	if _t0.__name == "Leaf" then return 0 end
	if _t0.__name == "Node" then return (function(l, r) return ((depth(l) + depth(r)) + 1) end)(_t0.left, _t0.right) end
	return nil
end)()
end

__print(depth({__name="Node", left={__name="Leaf"}, value=0, right={__name="Leaf"}}))
