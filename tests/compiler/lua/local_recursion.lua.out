function __div(a, b)
	if math.type and math.type(a) == 'integer' and math.type(b) == 'integer' then
		return a // b
	end
	return a / b
end

function __index(obj, i)
	if type(obj) == 'string' then
		return __indexString(obj, i)
	elseif type(obj) == 'table' then
		return obj[(i)+1]
	else
		error('cannot index')
	end
end

function __indexString(s, i)
	local len = #s
	if i < 0 then
		i = len + i + 1
	else
		i = i + 1
	end
	if i < 1 or i > len then error('index out of range') end
	return string.sub(s, i, i)
end

function fromList(nums)
	local function helper(lo, hi)
		if (lo >= hi) then
			return {__name="Leaf"}
		end
		local mid = __div(((lo + hi)), 2)
		return {__name="Node", left=helper(lo, mid), value=__index(nums, mid), right=helper((mid + 1), hi)}
	end
	return helper(0, #nums)
end

function inorder(t)
	return (function()
	local _t0 = t
	if _t0.__name == "Leaf" then return {} end
	if _t0.__name == "Node" then return (function(l, v, r) return ((inorder(l) + {v}) + inorder(r)) end)(_t0.left, _t0.value, _t0.right) end
	return nil
end)()
end

print(inorder(fromList({-10, -3, 0, 5, 9})))
