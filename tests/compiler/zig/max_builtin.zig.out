const std = @import("std");

fn _max_int(v: []const i32) i32 {
    if (v.len == 0) return 0;
    var m: i32 = v[0];
    for (v[1..]) |it| { if (it > m) m = it; }
    return m;
}

pub fn main() void {
    std.debug.print("{any}\n", .{_max_int(&[_]i32{@as(i32,@intCast(1)), @as(i32,@intCast(2)), @as(i32,@intCast(3))})});
}
