type Person {
  name: string
  age: int
  email: string
}
let people = load "tests/compiler/zig/people.json" as Person with {
  format: "json",
}
let adults = from p in people
             where p.age >= 18
             select { name: p.name, email: p.email }
for a in adults {
  print(a.name, a.email)
}
