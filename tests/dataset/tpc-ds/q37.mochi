let item = [
  {i_item_sk: 1, i_item_id: "I1", i_item_desc: "Widget", i_current_price: 30.0, i_manufact_id: 800},
  {i_item_sk: 2, i_item_id: "I2", i_item_desc: "Gadget", i_current_price: 60.0, i_manufact_id: 900}
]

let inventory = [
  {inv_item_sk: 1, inv_date_sk: 1, inv_quantity_on_hand: 200, inv_warehouse_sk: 1}
]

let date_dim = [
  {d_date_sk: 1, d_date: "2000-01-15"}
]

let catalog_sales = [
  {cs_item_sk: 1, cs_sold_date_sk: 1}
]

let result =
  from i in item
  join inv in inventory on inv.inv_item_sk == i.i_item_sk
  join d in date_dim on inv.inv_date_sk == d.d_date_sk
  join cs in catalog_sales on cs.cs_item_sk == i.i_item_sk
  where i.i_current_price >= 20 && i.i_current_price <= 50 &&
        d.d_date >= "2000-01-01" && d.d_date <= "2000-03-01" &&
        i.i_manufact_id in [800,801,802,803] &&
        inv.inv_quantity_on_hand >= 100 && inv.inv_quantity_on_hand <= 500
  group by {id: i.i_item_id, desc: i.i_item_desc, price: i.i_current_price} into g
  sort by g.key.id
  select {i_item_id: g.key.id, i_item_desc: g.key.desc, i_current_price: g.key.price}
  |> to_list

json(result)

test "TPCDS Q37 simplified" {
  expect result == [{i_item_id: "I1", i_item_desc: "Widget", i_current_price: 30.0}]
}
