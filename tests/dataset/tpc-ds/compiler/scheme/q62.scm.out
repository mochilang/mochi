)

(define web_sales (list (list (cons "days" 10)) (list (cons "days" 40)) (list (cons "days" 70)) (list (cons "days" 100)) (list (cons "days" 130))))
(define result (+ (* (_count web_sales) 12) 2))
(_json result)
(run-test "TPCDS Q62 simplified" test_TPCDS_Q62_simplified)
(when (> failures 0) (display "\n[FAIL] ") (display failures) (display " test(s) failed.\n"))
