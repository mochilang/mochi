from __future__ import annotations
import dataclasses
import json


@dataclasses.dataclass
class Sale:
    promo: bool
    price: int

    def __getitem__(self, key):
        return getattr(self, key)


def test_TPCDS_Q61_simplified():
    assert result == 61


sales = [
    Sale(promo=True, price=20),
    Sale(promo=True, price=41),
    Sale(promo=False, price=39),
]
promotions = sum([s.price for s in sales if s.promo])
total = sum([s.price for s in sales])
result = promotions * 100 / total
print(json.dumps(result, default=lambda o: vars(o)))
test_TPCDS_Q61_simplified()