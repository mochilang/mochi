open System
open System.Text.Json

type Anon1 = {
    time_sk: int
    hour: int
    minute: int
}
type Anon2 = {
    hd_demo_sk: int
    hd_dep_count: int
    hd_vehicle_count: int
}
type Anon3 = {
    s_store_sk: int
    s_store_name: string
}
type Anon4 = {
    sold_time_sk: int
    hdemo_sk: int
    store_sk: int
    qty: float
}
let time_dim: Anon1 list = [{ time_sk = 1; hour = 8; minute = 30 }; { time_sk = 2; hour = 9; minute = 0 }; { time_sk = 3; hour = 9; minute = 30 }; { time_sk = 4; hour = 10; minute = 0 }; { time_sk = 5; hour = 10; minute = 30 }; { time_sk = 6; hour = 11; minute = 0 }; { time_sk = 7; hour = 11; minute = 30 }; { time_sk = 8; hour = 12; minute = 0 }]
let household_demographics: Anon2 list = [{ hd_demo_sk = 1; hd_dep_count = 1; hd_vehicle_count = 1 }]
let store: Anon3 list = [{ s_store_sk = 1; s_store_name = "ese" }]
let store_sales: Anon4 list = [{ sold_time_sk = 1; hdemo_sk = 1; store_sk = 1; qty = 10.0 }; { sold_time_sk = 2; hdemo_sk = 1; store_sk = 1; qty = 12.0 }; { sold_time_sk = 3; hdemo_sk = 1; store_sk = 1; qty = 14.0 }; { sold_time_sk = 4; hdemo_sk = 1; store_sk = 1; qty = 11.0 }; { sold_time_sk = 5; hdemo_sk = 1; store_sk = 1; qty = 8.0 }; { sold_time_sk = 6; hdemo_sk = 1; store_sk = 1; qty = 9.0 }; { sold_time_sk = 7; hdemo_sk = 1; store_sk = 1; qty = 10.0 }; { sold_time_sk = 8; hdemo_sk = 1; store_sk = 1; qty = 14.0 }]
let count_range (ssales) (tdim) (hour) (start_min) (end_min) =
    let mutable total: float = 0.0
    for ss in ssales do
        for t in tdim do
            if ss.sold_time_sk = t.time_sk && t.hour = hour && t.minute >= start_min && t.minute < end_min then
                total <- total + ss.qty
    total
let h8_30_to_9: obj = count_range store_sales time_dim 8 30 60
let h9_to_9_30: obj = count_range store_sales time_dim 9 0 30
let h9_30_to_10: obj = count_range store_sales time_dim 9 30 60
let h10_to_10_30: obj = count_range store_sales time_dim 10 0 30
let h10_30_to_11: obj = count_range store_sales time_dim 10 30 60
let h11_to_11_30: obj = count_range store_sales time_dim 11 0 30
let h11_30_to_12: obj = count_range store_sales time_dim 11 30 60
let h12_to_12_30: obj = count_range store_sales time_dim 12 0 30
let result: obj = h8_30_to_9 + h9_to_9_30 + h9_30_to_10 + h10_to_10_30 + h10_30_to_11 + h11_to_11_30 + h11_30_to_12 + h12_to_12_30
printfn "%A" (JsonSerializer.Serialize(result))
assert (result = 88)
