		Ca_address_sk: 2,
		Ca_zip:        "99999",
	}})
	item = _cast[[]ItemItem]([]ItemItem{ItemItem{
		I_item_sk: 1,
		I_item_id: "I1",
	}, ItemItem{
		I_item_sk: 2,
		I_item_id: "I2",
