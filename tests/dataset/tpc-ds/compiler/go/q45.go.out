								_item := map[string]any{}
								for k, v := range _cast[map[string]any](ws) {
									_item[k] = v
								}
								_item["ws"] = ws
								for k, v := range _cast[map[string]any](c) {
									_item[k] = v
								}
								_item["c"] = c
								for k, v := range _cast[map[string]any](ca) {
