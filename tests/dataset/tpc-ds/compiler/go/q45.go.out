								continue
							}
							if ((_contains(zip_list, _sliceString(ca.Ca_zip, 0, 5)) || _contains(item_ids, i.I_item_id)) && (d.D_qoy == qoy)) && (d.D_year == year) {
								key := ca.Ca_zip
								ks := fmt.Sprint(key)
