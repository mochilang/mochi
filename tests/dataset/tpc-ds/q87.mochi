fun distinct(xs: list<any>): list<any> {
  var out = []
  for x in xs {
    if !contains(out, x) {
      out = append(out, x)
    }
  }
  return out
}

fun concat(a: list<any>, b: list<any>): list<any> {
  var out = a
  for x in b {
    out = append(out, x)
  }
  return out
}

fun to_list(xs: list<any>): list<any> { return xs }

