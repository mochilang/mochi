let customer = [
  {c_customer_sk: 1, c_last_name: "Doe", c_first_name: "John"},
  {c_customer_sk: 2, c_last_name: "Smith", c_first_name: "Jane"},
]

let date_dim = [
  {d_date_sk: 1, d_month_seq: 1200, d_date: "2000-01-01"}
]

let store_sales = [
  {ss_sold_date_sk: 1, ss_customer_sk: 1}
]

let catalog_sales = [
  {cs_sold_date_sk: 1, cs_bill_customer_sk: 1}
]

let web_sales = [
  {ws_sold_date_sk: 1, ws_bill_customer_sk: 1}
]

let range_start = 1200
let range_end = 1211

let store_set = set(from ss in store_sales join d in date_dim on ss.ss_sold_date_sk == d.d_date_sk where d.d_month_seq >= range_start && d.d_month_seq <= range_end join c in customer on ss.ss_customer_sk == c.c_customer_sk select {last: c.c_last_name, first: c.c_first_name, date: d.d_date})

let catalog_set = set(from cs in catalog_sales join d in date_dim on cs.cs_sold_date_sk == d.d_date_sk where d.d_month_seq >= range_start && d.d_month_seq <= range_end join c in customer on cs.cs_bill_customer_sk == c.c_customer_sk select {last: c.c_last_name, first: c.c_first_name, date: d.d_date})

let web_set = set(from ws in web_sales join d in date_dim on ws.ws_sold_date_sk == d.d_date_sk where d.d_month_seq >= range_start && d.d_month_seq <= range_end join c in customer on ws.ws_bill_customer_sk == c.c_customer_sk select {last: c.c_last_name, first: c.c_first_name, date: d.d_date})

let inter = intersect(intersect(store_set, catalog_set), web_set)

let result = len(inter)

json(result)

test "TPCDS Q38 simplified" {
  expect result == 1
}
