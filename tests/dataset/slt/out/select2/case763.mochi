  select x.c)

let result = from row in t1
  order by [(row.a - row.b), (if row.c > sub0 { (row.a * 2) } else { (row.b * 10) }), (row.c - row.d), row.b]
  select [(row.a - row.b), (if row.c > sub0 { (row.a * 2) } else { (row.b * 10) }), (row.c - row.d), row.b]
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
