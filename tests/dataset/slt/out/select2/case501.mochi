/* SELECT e, a+b*2+c*3, a+b*2, a, abs(b-c), CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END, b FROM t1 WHERE b>c */
let sub0 = avg(from x in t1
  select x.c)

var result = from row in t1
  where row.b > row.c
  select [row.e, ((row.a + (row.b * 2)) + (row.c * 3)), (row.a + (row.b * 2)), row.a, (if (row.b - row.c) < 0 { -((row.b - row.c)) } else { (row.b - row.c) }), (if row.c > sub0 { (row.a * 2) } else { (row.b * 10) }), row.b]
result = from row in result
  order by join(from v in row select str(v), " " )
  select row
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
flatResult = from x in flatResult
  order by str(x)
  select x
for x in flatResult {
  print(x)
}
