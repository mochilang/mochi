let sub0 = avg(from x in t1
  select x.c)

let result = from row in t1
  where (null != 0 || row.c > row.d)
  order by [count(from x in t1
  where x.b < row.b
  select x), (if row.c > sub0 { (row.a * 2) } else { (row.b * 10) }), row.b]
  select [count(from x in t1
  where x.b < row.b
  select x), (if row.c > sub0 { (row.a * 2) } else { (row.b * 10) }), row.b]
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
