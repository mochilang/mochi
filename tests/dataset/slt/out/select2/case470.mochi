let sub0 = avg(from x in t1
  select x.c)

var result = from row in t1
  select [if row.a < row.b - 3 then 111 else if row.a <= row.b then 222 else if row.a < row.b + 3 then 333 else 444, count(from x in t1
  where x.b < row.b
  select x), row.d - row.e, row.a + row.b * 2, if row.c > sub0 then row.a * 2 else row.b * 10, count(from x in t1
  where x.c > row.c && x.d < row.d
  select x)]
result = from row in result
  order by join(from v in row select str(v), " " )
  select row
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
flatResult = from x in flatResult
  order by str(x)
  select x
