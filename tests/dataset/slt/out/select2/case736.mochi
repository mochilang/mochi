/* SELECT CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END, abs(b-c), abs(a) FROM t1 WHERE a>b OR c>d */
let sub0 = avg(from x in t1
  select x.c)

let result = from row in t1
  where (row.a > row.b || row.c > row.d)
  select [(if row.c > sub0 { (row.a * 2) } else { (row.b * 10) }), (if (row.b - row.c) < 0 { -((row.b - row.c)) } else { (row.b - row.c) }), (if row.a < 0 { -(row.a) } else { row.a })]
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
for x in flatResult {
  print(x)
}
