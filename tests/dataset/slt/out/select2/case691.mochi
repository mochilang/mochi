/* SELECT d-e, CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END FROM t1 WHERE c>d OR (c<=d-2 OR c>=d+2) OR e+d BETWEEN a+b-10 AND c+130 */
let sub0 = avg(from x in t1
  select x.c)

let result = from row in t1
  where ((row.c > row.d || ((row.c <= (row.d - 2) || row.c >= (row.d + 2)))) || ((row.e + row.d) >= ((row.a + row.b) - 10) && (row.e + row.d) <= (row.c + 130)))
  select [(row.d - row.e), (if row.c > sub0 { (row.a * 2) } else { (row.b * 10) })]
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
for x in flatResult {
  print(x)
