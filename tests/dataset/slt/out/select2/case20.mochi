  select x.c)

let result = from row in t1
  select [row.e, (if row.c > sub0 { (row.a * 2) } else { (row.b * 10) }), (row.d - row.e), row.d, (((row.a + (row.b * 2)) + (row.c * 3)) + (row.d * 4))]
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
