/*
# line: 187
# EVIDENCE-OF: R-40597-22164 The result of avg() is NULL if and only if
# there are no non-NULL inputs.
*/

type t1Row {
  x: int
  y: string
}

let t1 = [
  t1Row {
    x: 1,
    y: "true" + "",
  },
  t1Row {
    x: 0,
    y: "false" + "",
  },
  t1Row {
    x: null,
    y: null,
  },
  t1Row {
    x: 2,
    y: "true" + "",
  },
  t1Row {
    x: 2,
    y: "true" + "",
  },
]

/* SELECT avg(x) FROM t1 WHERE y='null' */
let result = avg(from row in t1
  where (row.y != null && "null" != null && row.y == "null")
  select row.x)
print(result)

test "case26" {
  expect result == null
}
