type t1Row {
  x: int
  y: string
}

let t1 = [
  t1Row {
    x: 1,
    y: "true" + "",
  },
  t1Row {
    x: 0,
    y: "false" + "",
  },
  t1Row {
    x: null,
    y: null,
  },
  t1Row {
    x: 2,
    y: "true" + "",
  },
  t1Row {
    x: 2,
    y: "true" + "",
  },
]

/* SELECT min(x) FROM t1 */
let result = min(from row in t1
  select row.x)
print(result)

test "case44" {
  expect result == 0
}
