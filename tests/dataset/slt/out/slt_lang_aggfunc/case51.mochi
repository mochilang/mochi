type t1Row {
  x: int
  y: string
}

let t1 = [
  t1Row {
    x: 1,
    y: "true" + "",
  },
  t1Row {
    x: 0,
    y: "false" + "",
  },
  t1Row {
    x: null,
    y: "NULL",
  },
  t1Row {
    x: 2,
    y: "true" + "",
  },
  t1Row {
    x: 2,
    y: "true" + "",
  },
]

/* SELECT total(x) FROM t1 */
var result = sum(from row in t1
  where row.x != null
  select row.x)
result = if result == null then 0.0 else (result as float)
print(result)

test "case51" {
  expect result == 5
}
