# EVIDENCE-OF: R-34280-42283 The count(X) function returns a count of
# the number of times that X is not NULL in a group.
*/

type t1Row {
  x: int
  y: string
}

let t1 = [
  t1Row {
    x: 1,
    y: "true" + "",
  },
  t1Row {
    x: 0,
    y: "false" + "",
  },
  t1Row {
    x: null,
    y: null,
  },
  t1Row {
    x: 2,
    y: "true" + "",
  },
  t1Row {
    x: 2,
    y: "true" + "",
  },
]

/* SELECT count(x) FROM t1 WHERE y='null' */
let result = count(from row in t1
  where (row.y != null && "null" != null && row.y == "null")
  select row.x)
print(result)

test "case28" {
  expect result == 0
}
