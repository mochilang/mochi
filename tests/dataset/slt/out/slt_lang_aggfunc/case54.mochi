  t1Row {
    x: 1,
    y: "true" + "",
  },
  t1Row {
    x: 0,
    y: "false" + "",
  },
  t1Row {
    x: null,
    y: null,
  },
  t1Row {
    x: 2,
    y: "true" + "",
  },
  t1Row {
    x: 2,
    y: "true" + "",
  },
]

/* SELECT sum(x) FROM t1 WHERE y='null' */
let result = sum(from row in t1
  where (row.y != null && "null" != null && row.y == "null")
  select row.x)
print(result)

