type t1Row {
  x: int
  y: string
}

let t1 = [
  t1Row {
    x: 1,
    y: "true" + "",
  },
  t1Row {
    x: 0,
    y: "false" + "",
  },
  t1Row {
    x: null,
    y: "NULL",
  },
  t1Row {
    x: 2,
    y: "true" + "",
  },
  t1Row {
    x: 2,
    y: "true" + "",
  },
]

/* SELECT total(y) FROM t1 */
var result = sum(from row in t1
  where row.y != null
  select row.y)
result = if result == null then 0.0 else (result as float)
print(result)

test "case13" {
  expect result == 0
