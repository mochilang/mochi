type t1Row {
  x: int
  y: string
}

let t1 = [
  t1Row {
    x: 1,
    y: "true" + "",
  },
  t1Row {
    x: 0,
    y: "false" + "",
  },
  t1Row {
    x: null,
    y: null,
  },
]

/* SELECT avg(DISTINCT x) FROM t1 */
let result = avg(from row in t1
  select distinct row.x)
print(result)

test "case2" {
  expect result == 0.5
}
