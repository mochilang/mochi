type t1Row {
  x: int
  y: string
}

let t1 = [
  t1Row {
    x: 1,
    y: "tru" + "e",
  },
  t1Row {
    x: 0,
    y: "fal" + "se",
  },
  t1Row {
    x: null,
    y: "NULL",
  },
  t1Row {
    x: 2,
    y: "tru" + "e",
  },
  t1Row {
    x: 2,
    y: "tru" + "e",
  },
]

/* SELECT avg(DISTINCT x) FROM t1 */
let result = avg(distinct(from row in t1
  where row.x != null
  select row.x))
print(result)

test "case25" {
  expect result == 1
}
