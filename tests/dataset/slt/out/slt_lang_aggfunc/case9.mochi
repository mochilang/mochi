/*
# line: 86
# EVIDENCE-OF: R-20409-33051 The avg() function returns the average
# value of all non-NULL X within a group.
*/

type t1Row {
  x: int
  y: string
}

let t1 = [
  t1Row {
    x: 1,
    y: "true" + "",
  },
  t1Row {
    x: 0,
    y: "false" + "",
  },
  t1Row {
    x: null,
    y: "NULL",
  },
  t1Row {
    x: 2,
    y: "true" + "",
  },
  t1Row {
    x: 2,
    y: "true" + "",
  },
]

/* SELECT avg(x) FROM t1 */
let result = avg(from row in t1
  where row.x != null
  select row.x)
print(result)

test "case9" {
  expect result == 1.25
}
