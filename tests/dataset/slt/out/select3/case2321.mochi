/* SELECT (a+b+c+d+e)/5, b, abs(b-c), c, CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END FROM t1 WHERE b>c OR c BETWEEN b-2 AND d+2 OR EXISTS(SELECT 1 FROM t1 AS x WHERE x.b<t1.b) */
let sub0 = avg(from x in t1
  select x.c)

var result = from row in t1
  where row.b > row.c || (row.c >= row.b - 2 && row.c <= row.d + 2) || count(from x in t1
  where x.b < row.b
  select x) > 0
  select [(row.a + row.b + row.c + row.d + row.e) / 5, row.b, if row.b - row.c < 0 then -(row.b - row.c) else row.b - row.c, row.c, if row.c > sub0 then row.a * 2 else row.b * 10]
result = from row in result
  order by join(from v in row select str(v), " " )
  select row
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
flatResult = from x in flatResult
  order by str(x)
  select x
for x in flatResult {
  print(x)
}
