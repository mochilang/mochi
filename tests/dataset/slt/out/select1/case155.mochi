/* SELECT abs(a), a-b, CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END, abs(b-c), d-e FROM t1 ORDER BY 3,1,4,5,2 */
let result = from row in t1
  order by (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10), abs(row["a"]), abs(row["b"] - row["c"]), row["d"] - row["e"], row["a"] - row["b"]
  select [abs(row["a"]), row["a"] - row["b"], (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10), abs(row["b"] - row["c"]), row["d"] - row["e"]]
let flatResult = from row in result
  from x in row
  select x
for x in flatResult {
  print(x)
