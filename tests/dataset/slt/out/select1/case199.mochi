/* SELECT a+b*2, c, CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END, d FROM t1 WHERE d NOT BETWEEN 110 AND 150 ORDER BY 1,3,2,4 */
let sub0 = avg(from x in t1
  select x.c)

let result = from row in t1
  where (row.d < 110 || row.d > 150)
  order by [(row.a + (row.b + row.b)), (if row.c > sub0 { (row.a + row.a) } else { (row.b * 10) }), row.c, row.d]
  select [(row.a + (row.b + row.b)), row.c, (if row.c > sub0 { (row.a + row.a) } else { (row.b * 10) }), row.d]
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
for x in flatResult {
  print(x)
}
