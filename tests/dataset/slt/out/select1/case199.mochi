/* SELECT a+b*2, c, CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END, d FROM t1 WHERE d NOT BETWEEN 110 AND 150 ORDER BY 1,3,2,4 */
let result = from row in t1
  where (row["d"] < 110 || row["d"] > 150)
  order by row["a"] + row["b"] * 2, (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10), row["c"], row["d"]
  select [row["a"] + row["b"] * 2, row["c"], (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10), row["d"]]
let flatResult = from row in result
  from x in row
  select x
for x in flatResult {
  print(x)
}

