let result = from row in t1
  where (row["c"] >= row["b"] - 2 && row["c"] <= row["d"] + 2)
  order by (row["a"] + row["b"] + row["c"] + row["d"] + row["e"]) / 5, row["a"] + row["b"] * 2 + row["c"] * 3 + row["d"] * 4 + row["e"] * 5, row["b"], row["e"], (row["a"] < row["b"] - 3 ? 111 : (row["a"] <= row["b"] ? 222 : (row["a"] < row["b"] + 3 ? 333 : 444))), row["a"] + row["b"] * 2 + row["c"] * 3, (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10)
  select [(row["a"] + row["b"] + row["c"] + row["d"] + row["e"]) / 5, row["b"], row["a"] + row["b"] * 2 + row["c"] * 3 + row["d"] * 4 + row["e"] * 5, (row["a"] < row["b"] - 3 ? 111 : (row["a"] <= row["b"] ? 222 : (row["a"] < row["b"] + 3 ? 333 : 444))), row["e"], (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10), row["a"] + row["b"] * 2 + row["c"] * 3]
let flatResult = from row in result
  from x in row
  select x
for x in flatResult {
