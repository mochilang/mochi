  where count(from x in t1
  where x["b"] < row["b"]
  select x) > 0
  order by row["d"] - row["e"], row["c"], row["a"] + row["b"] * 2 + row["c"] * 3 + row["d"] * 4 + row["e"] * 5, (row["c"] > avg(from x in t1
