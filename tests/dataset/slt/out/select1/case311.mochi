/* SELECT a+b*2+c*3+d*4, a, c-d, abs(b-c), b, CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END FROM t1 WHERE (e>c OR e<d) OR (c<=d-2 OR c>=d+2) ORDER BY 4,3,2,5,1,6 */
let result = from row in t1
  where (((row["e"] > row["c"] || row["e"] < row["d"])) || ((row["c"] <= row["d"] - 2 || row["c"] >= row["d"] + 2)))
  order by abs(row["b"] - row["c"]), row["c"] - row["d"], row["a"], row["b"], row["a"] + row["b"] * 2 + row["c"] * 3 + row["d"] * 4, (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10)
  select [row["a"] + row["b"] * 2 + row["c"] * 3 + row["d"] * 4, row["a"], row["c"] - row["d"], abs(row["b"] - row["c"]), row["b"], (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10)]
let flatResult = from row in result
  from x in row
  select x
for x in flatResult {
  print(x)
