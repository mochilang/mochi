/* SELECT a+b*2+c*3+d*4+e*5, b-c, CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END FROM t1 WHERE a>b ORDER BY 3,1,2 */
let result = from row in t1
  where row["a"] > row["b"]
  order by (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10), row["a"] + row["b"] * 2 + row["c"] * 3 + row["d"] * 4 + row["e"] * 5, row["b"] - row["c"]
  select [row["a"] + row["b"] * 2 + row["c"] * 3 + row["d"] * 4 + row["e"] * 5, row["b"] - row["c"], (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10)]
let flatResult = from row in result
  from x in row
  select x
for x in flatResult {
  print(x)
}

