/* SELECT a-b, (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b), CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END FROM t1 ORDER BY 3,2,1 */
let sub0 = avg(from x in t1
  select x.c)

let result = from row in t1
  order by [(if row.c > sub0 { (row.a + row.a) } else { (row.b * 10) }), count(from x in t1
  where x.b < row.b
  select x), (row.a - row.b)]
  select [(row.a - row.b), count(from x in t1
  where x.b < row.b
  select x), (if row.c > sub0 { (row.a + row.a) } else { (row.b * 10) })]
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
for x in flatResult {
  print(x)
}
