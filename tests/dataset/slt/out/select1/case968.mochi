/* SELECT (a+b+c+d+e)/5, d-e FROM t1 WHERE c BETWEEN b-2 AND d+2 ORDER BY 2,1 */
let result = from row in t1
  where (row["c"] >= row["b"] - 2 && row["c"] <= row["d"] + 2)
  order by row["d"] - row["e"], (row["a"] + row["b"] + row["c"] + row["d"] + row["e"]) / 5
  select [(row["a"] + row["b"] + row["c"] + row["d"] + row["e"]) / 5, row["d"] - row["e"]]
let flatResult = from row in result
  from x in row
  select x
for x in flatResult {
  print(x)
}

