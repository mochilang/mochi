/* SELECT d, CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END, a-b FROM t1 WHERE EXISTS(SELECT 1 FROM t1 AS x WHERE x.b<t1.b) OR b>c OR a>b ORDER BY 1,3,2 */
let result = from row in t1
  where ((count(from x in t1
  where x["b"] < row["b"]
  select x) > 0 || row["b"] > row["c"]) || row["a"] > row["b"])
  order by row["d"], row["a"] - row["b"], (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10)
  select [row["d"], (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10), row["a"] - row["b"]]
let flatResult = from row in result
  from x in row
  select x
for x in flatResult {
  print(x)
}

