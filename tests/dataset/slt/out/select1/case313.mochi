let result = from row in t1
  order by row["a"], (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10), row["e"], row["b"], row["a"] - row["b"], row["a"] + row["b"] * 2 + row["c"] * 3 + row["d"] * 4 + row["e"] * 5
