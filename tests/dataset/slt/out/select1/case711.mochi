/* SELECT a+b*2, a-b, CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END FROM t1 WHERE (e>a AND e<b) AND d NOT BETWEEN 110 AND 150 AND d>e ORDER BY 3,1,2 */
let sub0 = avg(from x in t1
  select x.c)

let result = from row in t1
  where ((((row.e > row.a && row.e < row.b)) && (row.d < 110 || row.d > 150)) && row.d > row.e)
  order by [(if row.c > sub0 { (row.a + row.a) } else { (row.b * 10) }), (row.a + (row.b + row.b)), (row.a - row.b)]
  select [(row.a + (row.b + row.b)), (row.a - row.b), (if row.c > sub0 { (row.a + row.a) } else { (row.b * 10) })]
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
for x in flatResult {
  print(x)
}
