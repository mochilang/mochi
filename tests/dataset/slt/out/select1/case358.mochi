/* SELECT c, (SELECT count(*) FROM t1 AS x WHERE x.c>t1.c AND x.d<t1.d), CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END, a+b*2+c*3+d*4 FROM t1 WHERE b>c OR (e>c OR e<d) OR d NOT BETWEEN 110 AND 150 ORDER BY 3,2,1,4 */
let sub0 = avg(from x in t1
  select x.c)

var result = from row in t1
  where ((row.b > row.c || ((row.e > row.c || row.e < row.d))) || (row.d < 110 || row.d > 150))
  order by [(if row.c > sub0 { (row.a * 2) } else { (row.b * 10) }), count(from x in t1
  where (x.c > row.c && x.d < row.d)
  select x), row.c, (((row.a + (row.b * 2)) + (row.c * 3)) + (row.d * 4))]
  select [row.c, count(from x in t1
  where (x.c > row.c && x.d < row.d)
  select x), (if row.c > sub0 { (row.a * 2) } else { (row.b * 10) }), (((row.a + (row.b * 2)) + (row.c * 3)) + (row.d * 4))]
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
