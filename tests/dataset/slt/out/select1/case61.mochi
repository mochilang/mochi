let result = from row in t1
  order by [row.c - row.d, row.a + row.b * 2 + row.c * 3 + row.d * 4 + row.e * 5, (if row.a < 0 { -(row.a) } else { row.a })]
  select [(if row.a < 0 { -(row.a) } else { row.a }), row.a + row.b * 2 + row.c * 3 + row.d * 4 + row.e * 5, row.c - row.d]
var flatResult = []
