/* SELECT a+b*2+c*3, abs(a), e, c, CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END, d FROM t1 WHERE (e>c OR e<d) OR c BETWEEN b-2 AND d+2 OR a>b ORDER BY 5,2,4,1,3,6 */
let result = from row in t1
  where ((((row["e"] > row["c"] || row["e"] < row["d"])) || (row["c"] >= row["b"] - 2 && row["c"] <= row["d"] + 2)) || row["a"] > row["b"])
  order by (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10), abs(row["a"]), row["c"], row["a"] + row["b"] * 2 + row["c"] * 3, row["e"], row["d"]
  select [row["a"] + row["b"] * 2 + row["c"] * 3, abs(row["a"]), row["e"], row["c"], (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10), row["d"]]
let flatResult = from row in result
  from x in row
  select x
for x in flatResult {
  print(x)
}

