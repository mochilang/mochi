let result = from row in t1
  where (((row["e"] > row["c"] || row["e"] < row["d"])) || ((row["c"] <= row["d"] - 2 || row["c"] >= row["d"] + 2)))
  order by row["a"], abs(row["a"]), row["a"] + row["b"] * 2, row["d"] - row["e"], (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10)
  select [(row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10), abs(row["a"]), row["a"] + row["b"] * 2, row["a"], row["d"] - row["e"]]
let flatResult = from row in result
  from x in row
  select x
for x in flatResult {
  print(x)
}

