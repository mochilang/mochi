/* SELECT abs(a), CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END, c-d, b-c, b, (a+b+c+d+e)/5 FROM t1 WHERE b>c AND c BETWEEN b-2 AND d+2 ORDER BY 4,2,5,6,1,3 */
let result = from row in t1
  where (row["b"] > row["c"] && (row["c"] >= row["b"] - 2 && row["c"] <= row["d"] + 2))
  order by row["b"] - row["c"], (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10), row["b"], (row["a"] + row["b"] + row["c"] + row["d"] + row["e"]) / 5, abs(row["a"]), row["c"] - row["d"]
  select [abs(row["a"]), (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10), row["c"] - row["d"], row["b"] - row["c"], row["b"], (row["a"] + row["b"] + row["c"] + row["d"] + row["e"]) / 5]
let flatResult = from row in result
  from x in row
  select x
for x in flatResult {
  print(x)
}

