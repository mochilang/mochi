  where (row["a"] > row["b"] || row["b"] > row["c"])
  order by (row["a"] < row["b"] - 3 ? 111 : (row["a"] <= row["b"] ? 222 : (row["a"] < row["b"] + 3 ? 333 : 444))), row["d"], row["c"], row["a"] + row["b"] * 2 + row["c"] * 3, row["b"], row["d"] - row["e"]
