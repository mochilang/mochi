/* SELECT a-b, abs(a), d FROM t1 WHERE EXISTS(SELECT 1 FROM t1 AS x WHERE x.b<t1.b) OR d>e OR c BETWEEN b-2 AND d+2 ORDER BY 3,2,1 */
var result = from row in t1
  where count(from x in t1
  where x.b < row.b
  select x) > 0 || row.d > row.e || (row.c >= row.b - 2 && row.c <= row.d + 2)
  order by [row.d, if row.a < 0 then -(row.a) else row.a, row.a - row.b]
  select [row.a - row.b, if row.a < 0 then -(row.a) else row.a, row.d]
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
