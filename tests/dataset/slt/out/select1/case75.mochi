/* SELECT b, c, a-b, d-e, a+b*2+c*3+d*4 FROM t1 WHERE d>e OR c BETWEEN b-2 AND d+2 ORDER BY 2,1,4,3,5 */
let result = from row in t1
  where (row.d > row.e || (row.c >= row.b - 2 && row.c <= row.d + 2))
  order by [row.c, row.b, row.d - row.e, row.a - row.b, row.a + row.b * 2 + row.c * 3 + row.d * 4]
  select [row.b, row.c, row.a - row.b, row.d - row.e, row.a + row.b * 2 + row.c * 3 + row.d * 4]
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
for x in flatResult {
  print(x)
}

