/* SELECT c, a+b*2+c*3+d*4, a FROM t1 WHERE (e>a AND e<b) OR c BETWEEN b-2 AND d+2 ORDER BY 1,2,3 */
var result = from row in t1
  where (row.e > row.a && row.e < row.b) || (row.c >= row.b - 2 && row.c <= row.d + 2)
  order by [row.c, row.a + row.b * 2 + row.c * 3 + row.d * 4, row.a]
  select [row.c, row.a + row.b * 2 + row.c * 3 + row.d * 4, row.a]
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
