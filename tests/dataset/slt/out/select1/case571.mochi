  select [row["a"] + row["b"] * 2 + row["c"] * 3 + row["d"] * 4, (row["a"] < row["b"] - 3 ? 111 : (row["a"] <= row["b"] ? 222 : (row["a"] < row["b"] + 3 ? 333 : 444))), (row["c"] > avg(from x in t1
