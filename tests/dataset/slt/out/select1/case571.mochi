let result = from row in t1
  where ((row["c"] > row["d"] && (row["c"] >= row["b"] - 2 && row["c"] <= row["d"] + 2)) && ((row["e"] > row["c"] || row["e"] < row["d"])))
  order by count(from x in t1
  where (x["c"] > row["c"] && x["d"] < row["d"])
  select x), (row["a"] < row["b"] - 3 ? 111 : (row["a"] <= row["b"] ? 222 : (row["a"] < row["b"] + 3 ? 333 : 444))), row["a"] + row["b"] * 2 + row["c"] * 3, row["a"] + row["b"] * 2 + row["c"] * 3 + row["d"] * 4 + row["e"] * 5, row["a"] + row["b"] * 2 + row["c"] * 3 + row["d"] * 4, (row["c"] > avg(from x in t1
