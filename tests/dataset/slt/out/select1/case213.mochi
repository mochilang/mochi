let result = from row in t1
  where count(from x in t1
  where x["b"] < row["b"]
  select x) > 0
  order by row["b"], (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10), abs(row["b"] - row["c"]), row["d"] - row["e"], row["a"] - row["b"], row["b"] - row["c"], row["c"]
  select [row["b"], row["a"] - row["b"], row["c"], abs(row["b"] - row["c"]), row["d"] - row["e"], (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10), row["b"] - row["c"]]
let flatResult = from row in result
  from x in row
  select x
for x in flatResult {
  print(x)
}

