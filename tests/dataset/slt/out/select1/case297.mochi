  select x["c"]) ? row["a"] * 2 : row["b"] * 10), row["d"], abs(row["a"]), row["e"], row["a"] + row["b"] * 2]
let flatResult = from row in result
  from x in row
  select x
for x in flatResult {
  print(x)
}

