let result = from row in t1
  where (row["c"] >= row["b"] - 2 && row["c"] <= row["d"] + 2)
  order by abs(row["a"]), row["e"], row["d"], (row["c"] > avg(from x in t1
  select x["c"]) ? row["a"] * 2 : row["b"] * 10), row["a"] + row["b"] * 2
