// EVIDENCE-OF: R-40472-60438 Columns that do not appear in the list of
// assignments are left unmodified.

// SQL: SELECT count(*) FROM t1 WHERE y='unknown'

let t1 = [
  {
    x: 5,
    y: "unknown",
  },
  {
    x: 5,
    y: "false",
  },
  {
    x: 5,
    y: "NULL",
  },
]

let result = count(from row in t1
  where row.y == "unknown"
  select row)
print(result)

test "case7" {
  expect result == 1
}
