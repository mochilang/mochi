/* SELECT e, d-e, CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END, (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b) FROM t1 WHERE d NOT BETWEEN 110 AND 150 ORDER BY 1,3,4,2 */
let result = from row in t1
  where (row.d < 110 || row.d > 150)
  order by [row.e, (if row.c > avg(from x in t1
  select x.c) { row.a * 2 } else { row.b * 10 }), count(from x in t1
  where x.b < row.b
  select x), row.d - row.e]
  select [row.e, row.d - row.e, (if row.c > avg(from x in t1
  select x.c) { row.a * 2 } else { row.b * 10 }), count(from x in t1
  where x.b < row.b
  select x)]
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
for x in flatResult {
  print(x)
}
