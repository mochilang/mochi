let result = from row in t1
  where (row.a > row.b && (row.c >= row.b - 2 && row.c <= row.d + 2))
  order by [row.c - row.d, row.a + row.b * 2 + row.c * 3 + row.d * 4 + row.e * 5, count(from x in t1
