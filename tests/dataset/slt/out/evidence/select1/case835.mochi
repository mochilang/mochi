  where (row.c >= row.b - 2 && row.c <= row.d + 2)
  order by [(if row.a < row.b - 3 { 111 } else { (if row.a <= row.b { 222 } else { (if row.a < row.b + 3 { 333 } else { 444 }) }) }), row.a + row.b * 2 + row.c * 3 + row.d * 4, row.b - row.c, row.a - row.b]
