/* SELECT (SELECT count(*) FROM t1 AS x WHERE x.c>t1.c AND x.d<t1.d), a-b, b, CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END, e FROM t1 ORDER BY 1,5,4,3,2 */
let sub0 = avg(from x in t1
  select x.c)

let result = from row in t1
  order by [count(from x in t1
  where (x.c > row.c && x.d < row.d)
  select x), row.e, (if row.c > sub0 { (row.a * 2) } else { (row.b * 10) }), row.b, (row.a - row.b)]
  select [count(from x in t1
  where (x.c > row.c && x.d < row.d)
  select x), (row.a - row.b), row.b, (if row.c > sub0 { (row.a * 2) } else { (row.b * 10) }), row.e]
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
