let result = from row in t1
  order by [count(from x in t1
  where (x.c > row.c && x.d < row.d)
  select x), (if (row.a + 1) == row.b { 111 } else { (if (row.a + 1) == row.c { 222 } else { (if (row.a + 1) == row.d { 333 } else { (if (row.a + 1) == row.e { 444 } else { 555 }) }) }) }), count(from x in t1
  where x.b < row.b
  select x)]
  select [(if (row.a + 1) == row.b { 111 } else { (if (row.a + 1) == row.c { 222 } else { (if (row.a + 1) == row.d { 333 } else { (if (row.a + 1) == row.e { 444 } else { 555 }) }) }) }), count(from x in t1
  where x.b < row.b
  select x), count(from x in t1
  where (x.c > row.c && x.d < row.d)
  select x)]
var flatResult = []
