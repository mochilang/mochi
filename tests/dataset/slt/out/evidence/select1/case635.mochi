let result = from row in t1
  where (((row.a > row.b - 2 && row.a < row.b + 2)) || row.c > row.d)
  order by [row.b, row.d - row.e, row.a + row.b * 2 + row.c * 3 + row.d * 4 + row.e * 5, row.c, (row.a + row.b + row.c + row.d + row.e) / 5]
