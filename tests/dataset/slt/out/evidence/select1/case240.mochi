/* SELECT CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END, d, a+b*2+c*3 FROM t1 WHERE b>c OR c BETWEEN b-2 AND d+2 OR d NOT BETWEEN 110 AND 150 ORDER BY 1,3,2 */
let result = from row in t1
  where ((row.b > row.c || (row.c >= row.b - 2 && row.c <= row.d + 2)) || (row.d < 110 || row.d > 150))
  order by [(if row.c > avg(from x in t1
  select x.c) { row.a * 2 } else { row.b * 10 }), row.a + row.b * 2 + row.c * 3, row.d]
  select [(if row.c > avg(from x in t1
  select x.c) { row.a * 2 } else { row.b * 10 }), row.d, row.a + row.b * 2 + row.c * 3]
test "case240" {
  var flatResult = []
  for row in result {
    for x in row {
      flatResult = append(flatResult, x)
    }
  }
