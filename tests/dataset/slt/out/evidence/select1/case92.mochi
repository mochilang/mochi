/* SELECT (SELECT count(*) FROM t1 AS x WHERE x.c>t1.c AND x.d<t1.d), (a+b+c+d+e)/5, CASE WHEN a<b-3 THEN 111 WHEN a<=b THEN 222 WHEN a<b+3 THEN 333 ELSE 444 END FROM t1 WHERE c BETWEEN b-2 AND d+2 OR (c<=d-2 OR c>=d+2) OR c>d ORDER BY 1,3,2 */
let result = from row in t1
  where (((row.c >= row.b - 2 && row.c <= row.d + 2) || ((row.c <= row.d - 2 || row.c >= row.d + 2))) || row.c > row.d)
  order by [count(from x in t1
  where (x.c > row.c && x.d < row.d)
  select x), (if row.a < row.b - 3 { 111 } else { (if row.a <= row.b { 222 } else { (if row.a < row.b + 3 { 333 } else { 444 }) }) }), (row.a + row.b + row.c + row.d + row.e) / 5]
  select [count(from x in t1
  where (x.c > row.c && x.d < row.d)
  select x), (row.a + row.b + row.c + row.d + row.e) / 5, (if row.a < row.b - 3 { 111 } else { (if row.a <= row.b { 222 } else { (if row.a < row.b + 3 { 333 } else { 444 }) }) })]
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
for x in flatResult {
  print(x)
}

