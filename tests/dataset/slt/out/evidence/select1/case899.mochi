let result = from row in t1
  where ((row.c >= row.b - 2 && row.c <= row.d + 2) && row.d > row.e)
  order by [row.a - row.b, row.b, (if row.a < 0 { -(row.a) } else { row.a }), row.e, count(from x in t1
  where x.b < row.b
  select x), row.d, count(from x in t1
  where (x.c > row.c && x.d < row.d)
  select x)]
  select [row.b, row.a - row.b, row.e, row.d, (if row.a < 0 { -(row.a) } else { row.a }), count(from x in t1
  where x.b < row.b
  select x), count(from x in t1
  where (x.c > row.c && x.d < row.d)
  select x)]
var flatResult = []
for row in result {
  for x in row {
    flatResult = append(flatResult, x)
  }
}
