# occurrence is ignored.
skipif mssql
UPDATE t1 SET x=3, x=4, x=5
skipif mssql
*/

type t1Row {
  x: int
  y: string
}

var t1 = [
  t1Row {
    x: 4,
    y: "unknown",
  },
  t1Row {
    x: 4,
    y: "false" + "",
  },
  t1Row {
    x: 4,
    y: "NULL",
  },
]

/* UPDATE t1 SET x=3, x=4, x=5 */
update t1
set {
  x: 3,
  x: 4,
  x: 5
}

/* SELECT count(*) FROM t1 WHERE x=3 */
let result = count(from row in t1
  where row.x == 3
  select row)
print(result)

test "case4" {
  expect result == 0
}
