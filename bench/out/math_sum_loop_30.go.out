package main

import (
	"encoding/json"
	"fmt"
	"time"
)

func sum_loop(n int) int {
	var total int = 0
	for i := 1; i < n; i++ {
		total = (total + i)
	}
	return total
}

func main() {
	var n int = 30
	var repeat int = 1000
	var last int = 0
	var start int64 = time.Now().UnixNano()
	for i := 0; i < repeat; i++ {
		last = sum_loop(n)
	}
	var duration int64 = (int64(((int64(time.Now().UnixNano()) - int64(start)))) / int64(1000))
	func(){b,_:=json.Marshal(map[string]int64{"duration_us": duration, "output": int64(last)});fmt.Println(string(b))}()
}

