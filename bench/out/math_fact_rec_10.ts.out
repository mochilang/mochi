// Generated by Mochi TypeScript compiler

function fact_rec(n: number) : number {
	if ((n == 0)) {
		return 1
	}
	return (n * fact_rec((n - 1)))
}

function main(): void {
	let n: number = 10
	let repeat: number = 1000
	let last: number = 0
	let start: number = performance.now() * 1000000
	for (let i: number = 0; i < repeat; i++) {
		last = fact_rec(n)
	}
	let duration: number = Math.trunc(((performance.now() * 1000000 - start)) / 1000)
	console.log(JSON.stringify({"duration_us": duration, "output": last}))
}
main()

