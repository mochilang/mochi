fun sum(n: int): int {
  var total = 0
  for i in 1..n {
    total = total + i
  }
  return total
}

let n = {{ .N }}
let repeat = 1000
var last = 0

let start = now()
for i in 0..repeat {
  last = sum(n)
}
let duration = (now() - start) / 1000


json({
  "duration_us": duration,
  "output": last,
})
