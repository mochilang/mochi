// union.mochi
// Deduplicating union of two customer lists

let listA = [
  { id: 1, name: "Alice" },
  { id: 2, name: "Bob" }
]

let listB = [
  { id: 2, name: "Bob" },
  { id: 3, name: "Charlie" }
]

// Standard union (deduplicated)
let result = (from x in listA select x)
             union
             (from x in listB select x)

print("--- UNION (deduplicated) ---")
for x in result {
  print("Customer", x.id, "-", x.name)
}
