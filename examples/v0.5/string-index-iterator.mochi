let s = "hello world"

// Indexing
let first = s[0]
let fifth = s[4]
print("first character:", first)
print("fifth character:", fifth)

// Iteration
for ch in s {
  print("char:", ch)
}

// Count number of vowels
let vowels = "aeiou"
var count = 0

for ch in s {
  if ch in vowels {
    count = count + 1
  }
}
print("number of vowels:", count)
