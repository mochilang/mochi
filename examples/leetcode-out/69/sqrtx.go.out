package main

func expect(cond bool) {
	if !cond { panic("expect failed") }
}

func mySqrt(x int) int {
	if (x < 2) {
		return x
	}
	var left int = 1
	var right int = (x / 2)
	var ans int = 0
	for (left <= right) {
		var mid int = ((left + ((right - left))) / 2)
		var sq int = (mid * mid)
		if (sq == x) {
			return mid
		} else 		if (sq < x) {
			left = (mid + 1)
			ans = mid
		} else {
			right = (mid - 1)
		}
	}
	return ans
}

func example_1() {
	expect((mySqrt(4) == 2))
}

func example_2() {
	expect((mySqrt(8) == 2))
}

func zero() {
	expect((mySqrt(0) == 0))
}

func large() {
	expect((mySqrt(2147395599) == 46339))
}

func main() {
	example_1()
	example_2()
	zero()
	large()
}

