package main

func expect(cond bool) {
	if !cond { panic("expect failed") }
}

func twoSum(numbers []int, target int) []int {
	var left int = 0
	var right int = (len(numbers) - 1)
	for (left < right) {
		var sum int = (numbers[left] + numbers[right])
		if (sum == target) {
			return []int{(left + 1), (right + 1)}
		} else 		if (sum < target) {
			left = (left + 1)
		} else {
			right = (right - 1)
		}
	}
	return []any{}
}

func example_1() {
	expect((twoSum([]int{2, 7, 11, 15}, 9) == []int{1, 2}))
}

func example_2() {
	expect((twoSum([]int{2, 3, 4}, 6) == []int{1, 3}))
}

func example_3() {
	expect((twoSum([]int{-1, 0}, -1) == []int{1, 2}))
}

func main() {
	example_1()
	example_2()
	example_3()
}

