(define (isPalindrome x)
	(call/cc (lambda (return)
		(define n '())
		(define s '())
		(if (< x 0)
			(begin
				(return #f)
			)
			'()
		)
		(set! s (str x))
		(set! n (length s))
		(let loop ((i 0))
			(if (< i (/ n 2))
				(begin
					(if (not (= (list-ref s i) (list-ref s (- (- n 1) i))))
						(begin
							(return #f)
						)
						'()
					)
					(loop (+ i 1))
				)
			'())
		)
		(return #t)
	))
)

