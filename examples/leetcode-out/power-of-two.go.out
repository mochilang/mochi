package main

func expect(cond bool) {
	if !cond { panic("expect failed") }
}

func isPowerOfTwo(n int) bool {
	if (n <= 0) {
		return false
	}
	var num int = n
	_ = num
	for (num > 1) {
		if ((num % 2) != 0) {
			return false
		}
		num = (num / 2)
	}
	return true
}

func example_1() {
	expect((isPowerOfTwo(1) == true))
}

func example_2() {
	expect((isPowerOfTwo(16) == true))
}

func example_3() {
	expect((isPowerOfTwo(3) == false))
}

func zero() {
	expect((isPowerOfTwo(0) == false))
}

func negative() {
	expect((isPowerOfTwo(-2) == false))
}

func main() {
	example_1()
	example_2()
	example_3()
	zero()
	negative()
}

