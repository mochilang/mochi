package main

func expect(cond bool) {
	if !cond { panic("expect failed") }
}

func removeDuplicates(nums []int) int {
	var n int = len(nums)
	if (n <= 2) {
		return n
	}
	var write int = 2
	var read int = 2
	for (read < n) {
		if (nums[read] != nums[(write - 2)]) {
			nums[write] = nums[read]
			write = (write + 1)
		}
		read = (read + 1)
	}
	return write
}

func example_1() {
	var nums []int = []int{1, 1, 1, 2, 2, 3}
	var k int = removeDuplicates(nums)
	expect((k == 5))
	expect((nums[0:k] == []int{1, 1, 2, 2, 3}))
}

func example_2() {
	var nums []int = []int{0, 0, 1, 1, 1, 1, 2, 3, 3}
	var k int = removeDuplicates(nums)
	expect((k == 7))
	expect((nums[0:k] == []int{0, 0, 1, 1, 2, 3, 3}))
}

func main() {
	example_1()
	example_2()
}

