package main

func expect(cond bool) {
	if !cond { panic("expect failed") }
}

func permute(nums []int) [][]int {
	var result []any = []any{}
	backtrack([]any{}, nums)
	return result
}

func example_1() {
	expect((permute([]int{1, 2, 3}) == [][]int{[]int{1, 2, 3}, []int{1, 3, 2}, []int{2, 1, 3}, []int{2, 3, 1}, []int{3, 1, 2}, []int{3, 2, 1}}))
}

func example_2() {
	expect((permute([]int{0, 1}) == [][]int{[]int{0, 1}, []int{1, 0}}))
}

func example_3() {
	expect((permute([]int{1}) == [][]int{[]int{1}}))
}

func main() {
	example_1()
	example_2()
	example_3()
}

