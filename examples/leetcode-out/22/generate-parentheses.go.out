package main

func expect(cond bool) {
	if !cond { panic("expect failed") }
}

func generateParenthesis(n int) []string {
	var result []any = []any{}
	backtrack("", 0, 0)
	return result
}

func example_1() {
	expect((generateParenthesis(3) == []string{"((()))", "(()())", "(())()", "()(())", "()()()"}))
}

func example_2() {
	expect((generateParenthesis(1) == []string{"()"}))
}

func two_pairs() {
	expect((generateParenthesis(2) == []string{"(())", "()()"}))
}

func main() {
	example_1()
	example_2()
	two_pairs()
}

